22→\n  1923→        // --- Mobile Menu Toggle ---\n  1924→        const mobileToggle = document.getElementById('mobileToggle');\n  1925→        const navList = document.getElementById('navList');\n  1926→\n  1927→        mobileToggle.addEventListener('click', () => {\n  1928→            navList.classList.toggle('active');\n  1929→        });\n  1930→\n  1931→        // Close mobile menu when clicking a link\n  1932→        document.querySelectorAll('.nav-link').forEach(link => {\n  1933→            link.addEventListener('click', () => {\n  1934→                navList.classList.remove('active');\n  1935→            });\n  1936→        });\n  1937→\n  1938→        // --- Smooth Scroll ---\n  1939→        document.querySelectorAll('a[href^=\"#\"]').forEach(anchor => {\n  1940→            anchor.addEventListener('click', function (e) {\n  1941→                e.preventDefault();\n  1942→                const target = document.querySelector(this.getAttribute('href'));\n  1943→                if (target) {\n  1944→                    const headerOffset = 80;\n  1945→                    const elementPosition = target.getBoundingClientRect().top;\n  1946→                    const offsetPosition = elementPosition + window.pageYOffset - headerOffset;\n  1947→\n  1948→                    window.scrollTo({\n  1949→                        top: offsetPosition,\n  1950→                        behavior: 'smooth'\n  1951→                    });\n  1952→                }\n  1953→            });\n  1954→        });\n  1955→\n  1956→        // --- Intersection Observer for Animations ---\n  1957→        const observerOptions = {\n  1958→            root: null,\n  1959→            rootMargin: '0px',\n  1960→            threshold: 0.1\n  1961→        };\n  1962→\n  1963→        const observer = new IntersectionObserver((entries) => {\n  1964→            entries.forEach(entry => {\n  1965→                if (entry.isIntersecting) {\n  1966→                    entry.target.classList.add('animated');\n  1967→\n  1968→                    // Handle delay attribute\n  1969→                    const delay = entry.target.getAttribute('data-delay');\n  1970→                    if (delay) {\n  1971→                        entry.target.style.setProperty('--delay', delay + 'ms');\n  1972→                    }\n  1973→\n  1974→                    observer.unobserve(entry.target);\n  1975→                }\n  1976→            });\n  1977→        }, observerOptions);\n  1978→\n  1979→        // Observe all animated elements\n  1980→        document.querySelectorAll('.wow-fade-up, .wow-fade-in').forEach(el => {\n  1981→            observer.observe(el);\n  1982→        });\n  1983→\n  1984→        // --- Form Handling ---\n  1985→        const contactForm = document.getElementById('contactForm');\n  1986→\n  1987→        contactForm.addEventListener('submit', (e) => {\n  1988→            e.preventDefault();\n  1989→\n  1990→            const formData = new FormData(contactForm);\n  1991→            const data = Object.fromEntries(formData.entries());\n  1992→\n  1993→            // Log the form data (in production, send to backend)\n  1994→            console.log('Form submitted:', data);\n  1995→\n  1996→            // Show success message\n  1997→            alert('Obrigado pelo contato! Em breve entraremos em contato.');\n  1998→            contactForm.reset();\n  1999→        });\n  2000→\n  2001→        // --- Phone Number Click to Call ---\n  2002→        document.querySelectorAll('a[href^=\"tel:\"]').forEach(link => {\n  2003→            link.addEventListener('click', (e) => {\n  2004→                // Analytics tracking could go here\n  2005→                console.log('Phone call initiated');\n  2006→            });\n  2007→        });\n  2008→\n  2009→        // --- Emergency Button Pulse Animation ---\n  2010→        const emergencyBtn = document.querySelector('.nav-cta');\n  2011→        if (emergencyBtn) {\n  2012→            // Enhance visibility on