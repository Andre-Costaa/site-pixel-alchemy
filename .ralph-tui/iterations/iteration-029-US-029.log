  @keyframes pulse {\n            0%, 100% { opacity: 1; }\n            50% { opacity: 0.5; }\n        }\n\n        .hero-title {\n            color: var(--color-white);\n            margin-bottom: var(--spacing-md);\n        }\n\n        .hero-title span {\n            color: var(--color-accent-light);\n        }\n\n        .hero-description {\n            font-size: 1.125rem;\n            opacity: 0.9;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .hero-actions {\n            display: flex;\n            gap: 1rem;\n            flex-wrap: wrap;\n        }\n\n        .hero-phone {\n            display: flex;\n            align-items: center;\n            gap: 0.75rem;\n            margin-top: var(--spacing-md);\n            padding: 1rem 1.5rem;\n            background: rgba(255, 255, 255, 0.1);\n            backdrop-filter: blur(10px);\n            border-radius: var(--radius-lg);\n            border: 1px solid rgba(255, 255, 255, 0.2);\n        }\n\n        .hero-phone-icon {\n            width: 50px;\n            height: 50px;\n            background: var(--color-danger);\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            position: relative;\n        }\n\n        .hero-phone-icon::before {\n            content: '';\n            position: absolute;\n            width: 100%;\n            height: 100%;\n            border-radius: 50%;\n            background: var(--color-danger);\n            animation: pulse-ring 2s infinite;\n        }\n\n        .hero-phone-icon svg {\n            width: 24px;\n            height: 24px;\n            color: var(--color-white);\n            position: relative;\n            z-index: 1;\n        }\n\n        .hero-phone-content {\n            color: var(--color-white);\n        }\n\n        .hero-phone-label {\n            font-size: 0.875rem;\n            opacity: 0.8;\n        }\n\n        .hero-phone-number {\n    