               });\n                }\n            });\n        });\n\n        // --- Intersection Observer for Animations ---\n        const observerOptions = {\n            root: null,\n            rootMargin: '0px',\n            threshold: 0.1\n        };\n\n        const observer = new IntersectionObserver((entries) => {\n            entries.forEach(entry => {\n                if (entry.isIntersecting) {\n                    entry.target.classList.add('animated');\n\n                    // Handle delay attribute\n                    const delay = entry.target.getAttribute('data-delay');\n                    if (delay) {\n                        entry.target.style.setProperty('--delay', delay + 'ms');\n                    }\n\n                    observer.unobserve(entry.target);\n                }\n            });\n        }, observerOptions);\n\n        // Observe all animated elements\n        document.querySelectorAll('.wow-fade-up, .wow-fade-in').forEach(el => {\n            observer.observe(el);\n        });\n\n        // --- Form Handling ---\n        const contactForm = document.getElementById('contactForm');\n\n        contactForm.addEventListener('submit', (e) => {\n            e.preventDefault();\n\n            const formData = new FormData(contactForm);\n            const data = Object.fromEntries(formData.entries());\n\n            // Log the form data (in production, send to backend)\n            console.log('Form submitted:', data);\n\n            // Show success message\n            alert('Obrigado pelo contato! Em breve entraremos em contato.');\n            contactForm.reset();\n        });\n\n        // --- Phone Number Click to Call ---\n        document.querySelectorAll('a[href^=\"tel:\"]').forEach(link => {\n            link.addEventListener('click', (e) => {\n                // Analytics tracking could go here\n                console.log('Phone call initiated');\n            });\n        });\n\n        // --- Emergency Button Pulse Animation ---\n        const emergencyBtn = document.querySelector('.nav-cta');\n        if (emergencyBtn) {\n            // Enhance visibility on scroll\n            window.addEventListener('scroll', () => {\n                if (window.pageYOffset > 500) {\n                    emergencyBtn.style.animation = 'heartbeat 1s infinite';\n                } else {\n                    emergencyBtn.style.animation = 'heartbeat 2s infinite';\n                }\n            });\n        }\n\n        // --- Lazy Loading Images ---\n        if ('loading' in HTMLImageElement.prototype) {\n            const images = document.querySelectorAll('img[loading=\"lazy\"]');\n            images.forEach(img => {\n                img.src = img.dataset.src || img.src;\n            });\n        }\n\n        // --- Console Welcome Message ---\n        console.log('%c🐾 Dog Days - Clínica Veterinária 24h', 'font-size: 20px; font-weight: bold; color: #0d9488;');\n        console.log('%cAtendimento 24h em Ribeirão Preto', 'font-size: 14px; color: #64748b;');\n        console.log('%c📞 (16) 99147-8768', 'font-size: 14px; color: #f97316;');\n    </script>\n</body>\n</html>\n"}}],"stop_reason":null,"stop_sequence":null,"usage":{"input_tokens":0,"output_tokens":0},"context_management":null},"parent_tool_use_id":null,"session_id":"c9c44eb4-6964-4be6-8b24-f5bffa850384","uuid":"98a8ee97-79ef-49ef-9137-303497388c24"}
