            color: var(--color-terracotta);\n        }\n\n        .arrow-divider {\n            display: flex;\n            justify-content: center;\n            align-items: center;\n        }\n\n        .arrow-divider svg {\n            width: 40px;\n            height: 40px;\n            color: var(--color-terracotta);\n        }\n\n        .list-check {\n            list-style: none;\n            margin-top: var(--spacing-sm);\n        }\n\n        .list-check li {\n            display: flex;\n            align-items: flex-start;\n            gap: var(--spacing-xs);\n            margin-bottom: var(--spacing-xs);\n            font-size: 0.95rem;\n        }\n\n        .list-check svg {\n            width: 20px;\n            height: 20px;\n            flex-shrink: 0;\n            margin-top: 2px;\n        }\n\n        .problem-card .list-check svg {\n            color: var(--color-sage);\n        }\n\n        .solution-card .list-check svg {\n            color: var(--color-terracotta);\n        }\n\n        /* Services Section */\n        .services {\n            background: var(--color-forest);\n        }\n\n        .services-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n            gap: var(--spacing-md);\n        }\n\n        .service-card {\n            background: linear-gradient(180deg, var(--color-forest-light) 0%, var(--color-forest) 100%);\n            border: 1px solid rgba(255, 255, 255, 0.08);\n            border-radius: var(--radius-lg);\n            padding: var(--spacing-md);\n            position: relative;\n            overflow: hidden;\n            transition: all var(--transition-base);\n        }\n\n        .service-card:hover {\n            transform: translateY(-8px);\n            border-color: rgba(196, 90, 58, 0.3);\n            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.4);\n        }\n\n        .service-card::before {\n            content: '';\n            position: absolute;\n  