  1888→    <script>\n  1889→        // Navigation scroll effect\n  1890→        let lastScroll = 0;\n  1891→        const nav = document.getElementById('nav');\n  1892→\n  1893→        window.addEventListener('scroll', () => {\n  1894→            const currentScroll = window.pageYOffset;\n  1895→\n  1896→            if (currentScroll > 100) {\n  1897→                if (currentScroll > lastScroll) {\n  1898→                    nav.classList.add('hidden');\n  1899→                } else {\n  1900→                    nav.classList.remove('hidden');\n  1901→                }\n  1902→            }\n  1903→\n  1904→            lastScroll = currentScroll;\n  1905→        });\n  1906→\n  1907→        // Smooth scroll for anchor links\n  1908→        document.querySelectorAll('a[href^=\"#\"]').forEach(anchor => {\n  1909→            anchor.addEventListener('click', function (e) {\n  1910→                e.preventDefault();\n  1911→                const target = document.querySelector(this.getAttribute('href'));\n  1912→                if (target) {\n  1913→                    const navHeight = nav.offsetHeight;\n  1914→                    const targetPosition = target.offsetTop - navHeight;\n  1915→                    window.scrollTo({\n  1916→                        top: targetPosition,\n  1917→                        behavior: 'smooth'\n  1918→                    });\n  1919→                }\n  1920→            });\n  1921→        });\n  1922→\n  1923→        // Intersection Observer for scroll animations\n  1924→        const observerOptions = {\n  1925→            root: null,\n  1926→            rootMargin: '0px',\n  1927→            threshold: 0.1\n  1928→        };\n  1929→\n  1930→        const observer = new IntersectionObserver((entries) => {\n  1931→            entries.forEach(entry => {\n  1932→                if (entry.isIntersecting) {\n  1933→                    const delay = entry.target.dataset.delay || 0;\n  1934→                    setTimeout(() => {\n  1