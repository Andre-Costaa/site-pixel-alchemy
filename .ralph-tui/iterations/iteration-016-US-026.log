"#\"]').forEach(anchor => {\n  1753→            anchor.addEventListener('click', function(e) {\n  1754→                e.preventDefault();\n  1755→                const target = document.querySelector(this.getAttribute('href'));\n  1756→                if (target) {\n  1757→                    const headerOffset = 80;\n  1758→                    const elementPosition = target.getBoundingClientRect().top;\n  1759→                    const offsetPosition = elementPosition + window.pageYOffset - headerOffset;\n  1760→\n  1761→                    window.scrollTo({\n  1762→                        top: offsetPosition,\n  1763→                        behavior: 'smooth'\n  1764→                    });\n  1765→                }\n  1766→            });\n  1767→        });\n  1768→\n  1769→        // Form submission\n  1770→        const contactForm = document.getElementById('contactForm');\n  1771→        contactForm.addEventListener('submit', (e) => {\n  1772→            e.preventDefault();\n  1773→            alert('Obrigado pelo interesse! Em breve entraremos em contato para confirmar seu agendamento.');\n  1774→            contactForm.reset();\n  1775→        });\n  1776→\n  1777→        // Phone mask\n  1778→        const phoneInput = document.getElementById('phone');\n  1779→        phoneInput.addEventListener('input', (e) => {\n  1780→            let value = e.target.value.replace(/\\D/g, '');\n  1781→            if (value.length > 11) value = value.slice(0, 11);\n  1782→\n  1783→            if (value.length > 2) {\n  1784→                value = `(${value.slice(0, 2)}) ${value.slice(2)}`;\n  1785→            }\n  1786→            if (value.length > 10) {\n  1787→                value = `${value.slice(0, 10)}-${value.slice(10)}`;\n  1788→            }\n  1789→\n  1790→            e.target.value = value;\n  1791→        });\n  1792→    </script>\n  1793→</body>\n  1794→</html>\n  1795→"}]},"parent_tool_use_id":null,"session_id":"24cfff8c-0ba2-4d1f-9cbe-f731045280f6","uu