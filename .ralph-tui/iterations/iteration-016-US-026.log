9→            lastScroll = currentScroll;\n  2110→        });\n  2111→\n  2112→        // ===== MOBILE NAVIGATION =====\n  2113→        const mobileToggle = document.querySelector('.header__mobile-toggle');\n  2114→        const mobileNav = document.querySelector('.header__mobile-nav');\n  2115→        const mobileLinks = document.querySelectorAll('.header__mobile-nav a');\n  2116→\n  2117→        mobileToggle.addEventListener('click', () => {\n  2118→            mobileToggle.classList.toggle('active');\n  2119→            mobileNav.classList.toggle('active');\n  2120→            document.body.style.overflow = mobileNav.classList.contains('active') ? 'hidden' : '';\n  2121→        });\n  2122→\n  2123→        mobileLinks.forEach(link => {\n  2124→            link.addEventListener('click', () => {\n  2125→                mobileToggle.classList.remove('active');\n  2126→                mobileNav.classList.remove('active');\n  2127→                document.body.style.overflow = '';\n  2128→            });\n  2129→        });\n  2130→\n  2131→        // ===== INTERSECTION OBSERVER FOR ANIMATIONS =====\n  2132→        const observerOptions = {\n  2133→            threshold: 0.1,\n  2134→            rootMargin: '0px 0px -50px 0px'\n  2135→        };\n  2136→\n  2137→        const observer = new IntersectionObserver((entries) => {\n  2138→            entries.forEach(entry => {\n  2139→                if (entry.isIntersecting) {\n  2140→                    entry.target.classList.add('visible');\n  2141→                    observer.unobserve(entry.target);\n  2142→                }\n  2143→            });\n  2144→        }, observerOptions);\n  2145→\n  2146→        // Observe elements for animation\n  2147→        const animatedElements = document.querySelectorAll(\n  2148→            '.problem__content, .solution__card, .services__card, .differentials__item, .contact__info, .contact__form-wrapper'\n  2149→        );\n  2150→\n  2151→        animatedElements.forEach(el => observer.observe(el));\n  2152→\n  2153→        // ===== HERO ANIMATION ON LOAD =====\n  2154→        window.addEventListener('load', () => {\n  2155→            document.querySelector('.hero__content').classList.add('animate');\n  2156→            document.querySelector('.hero__image').classList.add('animate');\n  2157→        });\n  2158→\n  2159→        // ===== TESTIMONIALS CAROUSEL =====\n  2160→        const testimonialsTrack = document.getElementById('testimonialsTrack');\n  2161→        const testimonialsNav = document.getElementById('testimonialsNav');\n  2162→        const testimonialsItems = document.querySelectorAll('.testimonials__item');\n  2163→\n  2164→        // Create navigation dots\n  2165→        testimonialsItems.forEach((_, index) => {\n  2166→            const dot = document.createElement('button');\n  2167→            dot.className = 'testimonials__nav-dot' + (index === 0 ? ' active' : '');\n  2168→            dot.addEventListener('click', () => goToTestimonial(index));\n  2169→            testimonialsNav.appendChild(dot);\n  2170→        });\n  2171→\n  2172→        const navDots = document.querySelectorAll('.testimonials__nav-dot');\n  2173→\n  2174→        function goToTestimonial(index) {\n  2175→            testimonialsTrack.scrollTo({\n  2176→                left: testimonialsItems[index].offsetLeft - testimonialsTrack.offsetLeft,\n  2177→                behavior: 'smooth'\n  2178→            });\n  2179→        }\n  2180→\n  2181→        testimonialsTrack.addEventListener('scroll', () => {\n  2182→            const scrollLeft = testimonialsTrack.scrollLeft;\n  2183→            const itemWidth = testimonialsItems[0].offsetWidth;\n  2184→\n  2185→            navDots.forEach((dot, index) => {\n  2186→                if (scrollLeft >= index * itemWidth - itemWidth / 2 &&\n  2187→                    scrollLeft < (index + 1) * itemWidth - itemWidth / 2) {\n  2188→                    dot.classList.add('active');\n  2189→                } else