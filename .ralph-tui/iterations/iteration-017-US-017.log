         --font-body: 'Lato', sans-serif;\n\n            /* Transitions */\n            --transition-fast: 0.2s ease;\n            --transition-base: 0.3s ease;\n            --transition-slow: 0.5s ease;\n\n            /* Spacing */\n            --spacing-xs: 0.5rem;\n            --spacing-sm: 1rem;\n            --spacing-md: 1.5rem;\n            --spacing-lg: 2.5rem;\n            --spacing-xl: 4rem;\n            --spacing-2xl: 6rem;\n        }\n\n        * {\n            margin: 0;\n            padding: 0;\n            box-sizing: border-box;\n        }\n\n        html {\n            scroll-behavior: smooth;\n        }\n\n        body {\n            font-family: var(--font-body);\n            font-size: 16px;\n            line-height: 1.6;\n            color: var(--color-charcoal);\n            background-color: var(--color-cream);\n            overflow-x: hidden;\n        }\n\n        h1, h2, h3, h4, h5, h6 {\n            font-family: var(--font-display);\n            font-weight: 800;\n            line-height: 1.2;\n            color: var(--color-navy);\n        }\n\n        img {\n            max-width: 100%;\n            height: auto;\n            display: block;\n        }\n\n        a {\n            text-decoration: none;\n            color: inherit;\n            transition: var(--transition-fast);\n        }\n\n        ul {\n            list-style: none;\n        }\n\n        .container {\n            width: 100%;\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 0 var(--spacing-md);\n        }\n\n        .section {\n            padding: var(--spacing-2xl) 0;\n            position: relative;\n        }\n\n        /* Buttons */\n        .btn {\n            display: inline-flex;\n            align-items: center;\n            gap: var(--spacing-xs);\n            padding: 1rem 2rem;\n            font-family: var(--font-display);\n            font-size: 1rem;\n            font-weight: 700;\n            border-radius: 50px;\n            cursor: pointer;\n            transition: var(--transition-base);\n            border: none;\n            position: relative;\n            overflow: hidden;\n        }\n\n        .btn-primary {\n            background: linear-gradient(135deg, var(--color-coral) 0%, var(--color-coral-dark) 100%);\n            color: var(--color-white);\n            box-shadow: 0 8px 25px rgba(255, 123, 92, 0.35);\n        }\n\n        .btn-primary:hover {\n            transform: translateY(-3px);\n            box-shadow: 0 12px 35px rgba(255, 123, 92, 0.45);\n        }\n\n        .btn-secondary {\n            background: var(--color-white);\n            color: var(--color-coral);\n            border: 3px solid var(--color-coral);\n        }\n\n        .btn-secondary:hover {\n            background: var(--color-coral);\n            color: var(--color-white);\n        }\n\n        /* Navigation */\n        .nav {\n            position: fixed;\n            top: 0;\n            left: 0;\n            right: 0;\n            z-index: 1000;\n            padding: var(--spacing-sm) 0;\n            background: rgba(255, 255, 255, 0.95);\n            backdrop-filter: blur(20px);\n            transition: var(--transition-base);\n            box-shadow: 0 2px 20px rgba(0, 0, 0, 0.05);\n        }\n\n        .nav.hidden {\n            transform: translateY(-100%);\n        }\n\n        .nav-container {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n        }\n\n        .nav-logo {\n            font-family: var(--font-display);\n            font-size: 1.4rem;\n            font-weight: 900;\n            color: var(--color-coral);\n            display: flex;\n            align-items: center;\n            gap: 0.5rem;\n        }\n\n        .nav-logo svg {\n            width: 45px;\n            height: 45px;\n        }\n\n        .nav-logo span {\n            color: var(--color-teal);\n        }\n\n        .nav-menu {\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-lg);\n        }\n\n        .nav-link {\n            font-family: var(--font-display);\n            font-weight: 700;\n            color: var(--color-charcoal);\n            position: relative;\n        }\n\n        .nav-link::after {\n            content: '';\n            position: absolute;\n            bottom: -3px;\n            left: 0;\n            width: 0;\n            height: 3px;\n            background: var(--color-coral);\n            border-radius: 2px;\n            transition: var(--transition-fast);\n        }\n\n        .nav-link:hover::after {\n            width: 100%;\n        }\n\n        .nav-toggle {\n            display: none;\n            flex-direction: column;\n            gap: 5px;\n            cursor: pointer;\n            padding: 5px;\n        }\n\n        .nav-toggle span {\n            width: 25px;\n            height: 3px;\n            background: var(--color-coral);\n            border-radius: 3px;\n            transition: var(--transition-fast);\n        }\n\n        .nav-toggle.active span:nth-child(1) {\n            transform: rotate(45deg) translate(5px, 5px);\n        }\n\n        .nav-toggle.active span:nth-child(2) {\n            opacity: 0;\n        }\n\n        .nav-toggle.active span:nth-child(3) {\n            transform: rotate(-45deg) translate(5px, -5px);\n        }\n\n        /* Hero Section */\n        .hero {\n            min-height: 100vh;\n            padding-top: 90px;\n            display: flex;\n            align-items: center;\n            background: linear-gradient(180deg, var(--color-peach) 0%, var(--color-sand) 50%, var(--color-cream) 100%);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .hero-paw-bg {\n            position: absolute;\n            opacity: 0.03;\n            animation: pawFloat 25s ease-in-out infinite;\n        }\n\n        .hero-paw-bg svg {\n            width: 100%;\n            height: 100%;\n            fill: var(--color-coral);\n        }\n\n        .hero-paw-1 { top: 5%; left: 3%; width: 120px; animation-delay: 0s; }\n        .hero-paw-2 { top: 60%; right: 5%; width: 150px; animation-delay: 8s; }\n        .hero-paw-3 { bottom: 10%; left: 8%; width: 100px; animation-delay: 16s; }\n        .hero-paw-4 { top: 30%; right: 10%; width: 80px; animation-delay: 12s; }\n        .hero-paw-5 { bottom: 30%; left: 45%; width: 90px; animation-delay: 20s; }\n\n        @keyframes pawFloat {\n            0%, 100% { transform: translateY(0) rotate(0deg); }\n            25% { transform: translateY(-20px) rotate(5deg); }\n            50% { transform: translateY(-30px) rotate(0deg); }\n            75% { transform: translateY(-15px) rotate(-5deg); }\n        }\n\n        .hero-container {\n            display: grid;\n            grid-template-columns: 1.1fr 0.9fr;\n            gap: var(--spacing-xl);\n            align-items: center;\n            position: relative;\n            z-index: 1;\n        }\n\n        .hero-content {\n            animation: slideInLeft 1s ease;\n        }\n\n        @keyframes slideInLeft {\n            from { opacity: 0; transform: translateX(-40px); }\n            to { opacity: 1; transform: translateX(0); }\n        }\n\n        .hero-tag {\n            display: inline-flex;\n            align-items: center;\n            gap: var(--spacing-xs);\n            padding: 0.6rem 1.2rem;\n            background: var(--color-white);\n            border-radius: 50px;\n            font-family: var(--font-display);\n            font-size: 0.85rem;\n            font-weight: 700;\n            color: var(--color-coral);\n            margin-bottom: var(--spacing-md);\n            box-shadow: 0 4px 15px rgba(255, 123, 92, 0.15);\n        }\n\n        .hero-tag svg {\n            width: 18px;\n            height: 18px;\n        }\n\n        .hero-title {\n            font-size: clamp(2.5rem, 6vw, 4.2rem);\n            margin-bottom: var(--spacing-md);\n            line-height: 1.05;\n        }\n\n        .hero-title span {\n            color: var(--color-coral);\n            position: relative;\n            display: inline-block;\n        }\n\n        .hero-title span::after {\n            content: '';\n            position: absolute;\n            bottom: 8px;\n            left: 0;\n            right: 0;\n            height: 12px;\n            background: rgba(255, 123, 92, 0.2);\n            z-index: -1;\n            border-radius: 6px;\n        }\n\n        .hero-description {\n            font-size: 1.15rem;\n            color: var(--color-warm-gray);\n            margin-bottom: var(--spacing-lg);\n            max-width: 520px;\n            line-height: 1.8;\n        }\n\n        .hero-buttons {\n            display: flex;\n            gap: var(--spacing-sm);\n            flex-wrap: wrap;\n        }\n\n        .hero-image-wrapper {\n            position: relative;\n            animation: slideInRight 1s ease 0.2s both;\n        }\n\n        @keyframes slideInRight {\n            from { opacity: 0; transform: translateX(40px); }\n            to { opacity: 1; transform: translateX(0); }\n        }\n\n        .hero-main-image {\n            position: relative;\n            border-radius: 40px;\n            overflow: hidden;\n            box-shadow: 0 30px 70px rgba(26, 58, 64, 0.15);\n        }\n\n        .hero-main-image img {\n            width: 100%;\n            height: 520px;\n            object-fit: cover;\n        }\n\n        .hero-badge {\n            position: absolute;\n            background: var(--color-white);\n            border-radius: 25px;\n            padding: 1.2rem 1.5rem;\n            box-shadow: 0 15px 45px rgba(26, 58, 64, 0.12);\n            animation: badgeFloat 5s ease-in-out infinite;\n        }\n\n        .hero-badge.experience {\n            bottom: 35px;\n            left: -25px;\n        }\n\n        .hero-badge.pets {\n            top: 40px;\n            right: -15px;\n            animation-delay: 2s;\n        }\n\n        @keyframes badgeFloat {\n            0%, 100% { transform: translateY(0); }\n            50% { transform: translateY(-12px); }\n        }\n\n        .badge-icon {\n            width: 50px;\n            height: 50px;\n            background: linear-gradient(135deg, var(--color-coral), var(--color-coral-light));\n            border-radius: 15px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: var(--spacing-xs);\n        }\n\n        .badge-icon svg {\n            width: 24px;\n            height: 24px;\n            color: var(--color-white);\n        }\n\n        .badge-label {\n            font-family: var(--font-display);\n            font-size: 0.75rem;\n            font-weight: 700;\n            color: var(--color-warm-gray);\n            text-transform: uppercase;\n            letter-spacing: 1px;\n        }\n\n        .badge-value {\n            font-family: var(--font-display);\n            font-size: 1.8rem;\n            font-weight: 900;\n            color: var(--color-teal);\n        }\n\n        /* Problem/Solution Section */\n        .problem-solution {\n            background: var(--color-white);\n        }\n\n        .ps-container {\n            display: grid;\n            grid-template-columns: 1fr 1fr;\n            gap: var(--spacing-xl);\n        }\n\n        .ps-card {\n            padding: var(--spacing-xl);\n            border-radius: 35px;\n            position: relative;\n            overflow: hidden;\n        }\n\n        .ps-card.problem {\n            background: linear-gradient(135deg, var(--color-teal) 0%, var(--color-teal-dark) 100%);\n            color: var(--color-white);\n        }\n\n        .ps-card.solution {\n            background: linear-gradient(135deg, var(--color-coral) 0%, var(--color-coral-dark) 100%);\n            color: var(--color-white);\n        }\n\n        .ps-icon {\n            width: 75px;\n            height: 75px;\n            background: rgba(255, 255, 255, 0.2);\n            border-radius: 22px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .ps-icon svg {\n            width: 38px;\n            height: 38px;\n            color: var(--color-white);\n        }\n\n        .ps-card h3 {\n            color: var(--color-white);\n            font-size: 1.9rem;\n            margin-bottom: var(--spacing-sm);\n        }\n\n        .ps-card p {\n            opacity: 0.95;\n            font-size: 1.05rem;\n            line-height: 1.8;\n        }\n\n        .ps-card.solution .ps-list {\n            margin-top: var(--spacing-md);\n        }\n\n        .ps-list li {\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-sm);\n            margin-bottom: var(--spacing-sm);\n            font-weight: 600;\n            font-family: var(--font-display);\n        }\n\n        .ps-list li svg {\n            width: 22px;\n            height: 22px;\n            color: var(--color-peach);\n            flex-shrink: 0;\n        }\n\n        /* Services Section */\n        .services {\n            background: linear-gradient(180deg, var(--color-mint) 0%, var(--color-peach) 100%);\n        }\n\n        .section-header {\n            text-align: center;\n            margin-bottom: var(--spacing-xl);\n        }\n\n        .section-label {\n            display: inline-block;\n            font-family: var(--font-display);\n            font-size: 0.9rem;\n            font-weight: 700;\n            color: var(--color-coral);\n            text-transform: uppercase;\n            letter-spacing: 2px;\n            margin-bottom: var(--spacing-sm);\n        }\n\n        .section-title {\n            font-size: clamp(2rem, 5vw, 3rem);\n            margin-bottom: var(--spacing-sm);\n        }\n\n        .section-subtitle {\n            font-size: 1.1rem;\n            color: var(--color-warm-gray);\n            max-width: 650px;\n            margin: 0 auto;\n        }\n\n        .services-grid {\n            display: grid;\n            grid-template-columns: repeat(3, 1fr);\n            gap: var(--spacing-md);\n        }\n\n        .service-card {\n            background: var(--color-white);\n            border-radius: 30px;\n            padding: var(--spacing-lg);\n            transition: var(--transition-base);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .service-card::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            height: 5px;\n            background: linear-gradient(90deg, var(--color-coral), var(--color-teal));\n            transform: scaleX(0);\n            transition: var(--transition-base);\n        }\n\n        .service-card:hover {\n            transform: translateY(-10px);\n            box-shadow: 0 25px 55px rgba(26, 58, 64, 0.12);\n        }\n\n        .service-card:hover::before {\n            transform: scaleX(1);\n        }\n\n        .service-icon {\n            width: 70px;\n            height: 70px;\n            background: linear-gradient(135deg, rgba(255, 123, 92, 0.15), rgba(45, 154, 147, 0.15));\n            border-radius: 20px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .service-icon svg {\n            width: 34px;\n            height: 34px;\n            color: var(--color-coral);\n        }\n\n        .service-card h3 {\n            font-size: 1.4rem;\n            margin-bottom: var(--spacing-sm);\n        }\n\n        .service-card p {\n            color: var(--color-warm-gray);\n            font-size: 0.95rem;\n            line-height: 1.7;\n        }\n\n        /* Testimonials Section */\n        .testimonials {\n            background: var(--color-navy);\n            color: var(--color-white);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .testimonials::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            bottom: 0;\n            background:\n                radial-gradient(ellipse 35% 25% at 15% 25%, rgba(255, 123, 92, 0.12) 0%, transparent 50%),\n                radial-gradient(ellipse 30% 20% at 85% 75%, rgba(45, 154, 147, 0.1) 0%, transparent 50%);\n        }\n\n        .testimonials .section-title,\n        .testimonials .section-subtitle {\n            color: var(--color-white);\n        }\n\n        .testimonials .section-subtitle {\n            opacity: 0.85;\n        }\n\n        .testimonials-grid {\n            display: grid;\n            grid-template-columns: repeat(3, 1fr);\n            gap: var(--spacing-md);\n            position: relative;\n            z-index: 1;\n        }\n\n        .testimonial-card {\n            background: rgba(255, 255, 255, 0.06);\n            backdrop-filter: blur(12px);\n            border: 1px solid rgba(255, 255, 255, 0.1);\n            border-radius: 30px;\n            padding: var(--spacing-lg);\n            transition: var(--transition-base);\n        }\n\n        .testimonial-card:hover {\n            background: rgba(255, 255, 255, 0.1);\n            transform: translateY(-5px);\n        }\n\n        .testimonial-stars {\n            display: flex;\n            gap: 5px;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .testimonial-stars svg {\n            width: 22px;\n            height: 22px;\n            color: #ffd700;\n        }\n\n        .testimonial-text {\n            font-size: 1rem;\n            line-height: 1.75;\n            opacity: 0.9;\n            margin-bottom: var(--spacing-md);\n            font-style: italic;\n        }\n\n        .testimonial-author {\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-sm);\n        }\n\n        .testimonial-avatar {\n            width: 55px;\n            height: 55px;\n            border-radius: 50%;\n            background: linear-gradient(135deg, var(--color-coral), var(--color-teal));\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            font-family: var(--font-display);\n            font-weight: 800;\n            color: var(--color-white);\n            font-size: 1.1rem;\n        }\n\n        .testimonial-info h4 {\n            color: var(--color-white);\n            font-size: 1.05rem;\n            font-weight: 700;\n        }\n\n        .testimonial-info span {\n            font-size: 0.85rem;\n            opacity: 0.7;\n        }\n\n        /* Diferenciais Section */\n        .diferenciais {\n            background: var(--color-white);\n        }\n\n        .diferenciais-grid {\n            display: grid;\n            grid-template-columns: repeat(2, 1fr);\n            gap: var(--spacing-lg);\n        }\n\n        .diferencial-item {\n            display: flex;\n            align-items: flex-start;\n            gap: var(--spacing-md);\n            padding: var(--spacing-lg);\n            background: var(--color-peach);\n            border-radius: 25px;\n            transition: var(--transition-base);\n        }\n\n        .diferencial-item:hover {\n            background: var(--color-sand);\n            transform: translateX(8px);\n        }\n\n        .diferencial-icon {\n            width: 65px;\n            height: 65px;\n            background: linear-gradient(135deg, var(--color-coral), var(--color-coral-light));\n            border-radius: 18px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            flex-shrink: 0;\n        }\n\n        .diferencial-icon svg {\n            width: 30px;\n            height: 30px;\n            color: var(--color-white);\n        }\n\n        .diferencial-content h3 {\n            font-size: 1.3rem;\n            margin-bottom: var(--spacing-xs);\n        }\n\n        .diferencial-content p {\n            color: var(--color-warm-gray);\n            font-size: 0.95rem;\n            line-height: 1.7;\n        }\n\n        /* Contact Section */\n        .contact {\n            background: linear-gradient(135deg, var(--color-sand) 0%, var(--color-cream) 100%);\n        }\n\n        .contact-container {\n            display: grid;\n            grid-template-columns: 1fr 1.2fr;\n            gap: var(--spacing-xl);\n            align-items: start;\n        }\n\n        .contact-info {\n            background: var(--color-navy);\n            color: var(--color-white);\n            border-radius: 35px;\n            padding: var(--spacing-xl);\n        }\n\n        .contact-info h3 {\n            color: var(--color-white);\n            font-size: 2.1rem;\n            margin-bottom: var(--spacing-sm);\n        }\n\n        .contact-info > p {\n            opacity: 0.85;\n            margin-bottom: var(--spacing-lg);\n            line-height: 1.7;\n        }\n\n        .contact-item {\n            display: flex;\n            align-items: flex-start;\n            gap: var(--spacing-sm);\n            margin-bottom: var(--spacing-md);\n        }\n\n        .contact-item-icon {\n            width: 55px;\n            height: 55px;\n            background: rgba(255, 255, 255, 0.1);\n            border-radius: 15px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            flex-shrink: 0;\n        }\n\n        .contact-item-icon svg {\n            width: 26px;\n            height: 26px;\n            color: var(--color-coral-light);\n        }\n\n        .contact-item-text h4 {\n            color: var(--color-white);\n            font-size: 0.9rem;\n            font-weight: 600;\n            opacity: 0.7;\n            margin-bottom: 4px;\n        }\n\n        .contact-item-text p {\n            color: var(--color-white);\n            font-size: 1.05rem;\n            font-weight: 700;\n            margin: 0;\n            line-height: 1.5;\n        }\n\n        .contact-form {\n            background: var(--color-white);\n            border-radius: 35px;\n            padding: var(--spacing-xl);\n            box-shadow: 0 25px 65px rgba(26, 58, 64, 0.1);\n        }\n\n        .contact-form h3 {\n            font-size: 1.9rem;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .form-group {\n            margin-bottom: var(--spacing-md);\n        }\n\n        .form-group label {\n            display: block;\n            font-family: var(--font-display);\n            font-weight: 700;\n            margin-bottom: var(--spacing-xs);\n            color: var(--color-charcoal);\n        }\n\n        .form-group input,\n        .form-group select,\n        .form-group textarea {\n            width: 100%;\n            padding: 1rem 1.2rem;\n            border: 2px solid var(--color-sand);\n            border-radius: 15px;\n            font-family: var(--font-body);\n            font-size: 1rem;\n            transition: var(--transition-fast);\n            background: var(--color-cream);\n        }\n\n        .form-group input:focus,\n        .form-group select:focus,\n        .form-group textarea:focus {\n            outline: none;\n            border-color: var(--color-coral);\n            background: var(--color-white);\n        }\n\n        .form-group textarea {\n            min-height: 130px;\n            resize: vertical;\n        }\n\n        /* Footer */\n        .footer {\n            background: var(--color-charcoal);\n            color: var(--color-white);\n            padding: var(--spacing-xl) 0 var(--spacing-lg);\n        }\n\n        .footer-container {\n            display: grid;\n            grid-template-columns: 2fr 1fr 1fr 1fr;