font-weight: 700;\n            color: var(--color-deep-navy);\n            font-size: 0.9375rem;\n        }\n\n        .floating-card-sub {\n            font-size: 0.8125rem;\n            color: var(--color-soft-gray);\n        }\n\n        /* Problem/Solution Section */\n        .problem-solution {\n            background: white;\n            position: relative;\n        }\n\n        .problem-solution-grid {\n            display: grid;\n            grid-template-columns: repeat(2, 1fr);\n            gap: var(--spacing-xl);\n        }\n\n        .ps-card {\n            border-radius: var(--border-radius-large);\n            padding: var(--spacing-xl);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .ps-card::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            width: 6px;\n            height: 100%;\n        }\n\n        .problem-card {\n            background: linear-gradient(135deg, #fff5f3 0%, #ffe8e3 100%);\n        }\n\n        .problem-card::before {\n            background: var(--color-coral);\n        }\n\n        .solution-card {\n            background: linear-gradient(135deg, #f0f7f0 0%, #e5f0e5 100%);\n        }\n\n        .solution-card::before {\n            background: var(--color-sage);\n        }\n\n        .ps-header {\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-md);\n            margin-bottom: var(--spacing-lg);\n        }\n\n        .ps-icon {\n            width: 70px;\n            height: 70px;\n            border-radius: var(--border-radius-medium);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            flex-shrink: 0;\n        }\n\n        .problem-card .ps-icon {\n            background: rgba(232, 120, 106, 0.15);\n        }\n\n        .solution-card .ps-icon {\n            background: rgba(125, 168, 123, 0.15);\n        }\n\n        .ps-icon svg {\n            width: 32px;\n            height: 32px;\n        }\n\n        .problem-card .ps-icon svg {\n            fill: var(--color-coral);\n        }\n\n        .solution-card .ps-icon svg {\n            fill: var(--color-sage);\n        }\n\n        .ps-title {\n            font-family: var(--font-display);\n            font-size: 1.75rem;\n            font-weight: 700;\n            color: var(--color-deep-navy);\n        }\n\n        .ps-list {\n            margin-top: var(--spacing-md);\n        }\n\n        .ps-item {\n            display: flex;\n            align-items: flex-start;\n            gap: var(--spacing-sm);\n            margin-bottom: var(--spacing-md);\n            font-size: 1rem;\n            color: var(--color-soft-gray);\n            line-height: 1.7;\n        }\n\n        .ps-item svg {\n            width: 22px;\n            height: 22px;\n            flex-shrink: 0;\n            margin-top: 2px;\n        }\n\n        .problem-card .ps-item svg {\n            fill: var(--color-coral);\n        }\n\n        .solution-card .ps-item svg {\n            fill: var(--color-sage);\n        }\n\n        /* Services Section */\n        .services {\n            background: var(--color-warm-cream);\n            position: relative;\n        }\n\n        .section-header {\n            text-align: center;\n            margin-bottom: var(--spacing-2xl);\n        }\n\n        .section-tag {\n            display: inline-block;\n            background: var(--gradient-coral);\n            color: white;\n            padding: 0.5rem 1.5rem;\n            border-radius: var(--border-radius-pill);\n            font-size: 0.75rem;\n            font-weight: 700;\n            text-transform: uppercase;\n            letter-spacing: 0.15em;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .section-title {\n            font-family: var(--font-display);\n            font-size: clamp(2rem, 4.5vw, 3.25rem);\n            font-weight: 70