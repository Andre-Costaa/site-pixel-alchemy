bottom: -300px;\n            left: -300px;\n        }\n\n        .hero-inner {\n            display: grid;\n            grid-template-columns: 1fr 1fr;\n            gap: 80px;\n            align-items: center;\n            position: relative;\n            z-index: 1;\n        }\n\n        .hero-content {\n            opacity: 0;\n            transform: translateY(30px);\n            animation: fade-up 0.8s ease forwards;\n        }\n\n        @keyframes fade-up {\n            to {\n                opacity: 1;\n                transform: translateY(0);\n            }\n        }\n\n        .hero-badge {\n            display: inline-flex;\n            align-items: center;\n            gap: 10px;\n            background: rgba(220, 38, 38, 0.2);\n            border: 1px solid rgba(220, 38, 38, 0.3);\n            color: #FF8A8A;\n            padding: 10px 20px;\n            border-radius: var(--radius-full);\n            font-weight: 600;\n            font-size: 0.85rem;\n            margin-bottom: 28px;\n            text-transform: uppercase;\n            letter-spacing: 1px;\n        }\n\n        .hero-badge-dot {\n            width: 8px;\n            height: 8px;\n            background: var(--color-emergency);\n            border-radius: 50%;\n            animation: pulse-emergency 1.5s ease-in-out infinite;\n        }\n\n        @keyframes pulse-emergency {\n            0%, 100% { opacity: 1; transform: scale(1); box-shadow: 0 0 0 0 rgba(220, 38, 38, 0.7); }\n            50% { opacity: 0.8; transform: scale(1.1); box-shadow: 0 0 0 10px rgba(220, 38, 38, 0); }\n        }\n\n        .hero-title {\n            font-family: 'Space Grotesk', sans-serif;\n            font-size: clamp(2.5rem, 5.5vw, 4rem);\n            font-weight: 700;\n            line-height: 1.05;\n            color: white;\n            margin-bottom: 24px;\n            letter-spacing: -0.02em;\n        }\n\n        .hero-title-accent {\n            color: var(--color-accent);\n            position: relative;\n            display: inline-block;\n        }\n\n        .hero-title-accent::after {\n            content: '';\n            position: absolute;\n            bottom: 4px;\n            left: 0;\n            right: 0;\n            height: 4px;\n            background: var(--gradient-emergency);\n            border-radius: 2px;\n        }\n\n        .hero-description {\n            font-size: 1.2rem;\n            color: rgba(255, 255, 255, 0.7);\n            margin-bottom: 36px;\n            max-width: 520px;\n            line-height: 1.7;\n        }\n\n        .hero-actions {\n            display: flex;\n            gap: 16px;\n            flex-wrap: wrap;\n        }\n\n        .btn {\n            display: inline-flex;\n            align-items: center;\n            gap: 10px;\n            padding: 18px 36px;\n            border-radius: var(--radius-full);\n            font-weight: 700;\n            font-size: 1rem;\n            text-decoration: none;\n            transition: all var(--transition-fast);\n            cursor: pointer;\n            border: none;\n        }\n\n        .btn-primary {\n            background: var(--gradient-emergency);\n            color: white;\n            box-shadow: var(--shadow-glow);\n        }\n\n        .btn-primary:hover {\n            transform: translateY(-3px);\n            box-shadow: 0 8px 32px rgba(255, 107, 53, 0.4);\n        }\n\n        .btn-secondary {\n            background: rgba(255, 255, 255, 0.1);\n            color: white;\n            border: 2px solid rgba(255, 255, 255, 0.2);\n            backdrop-filter: blur(10px);\n        }\n\n        .btn-secondary:hover {\n            background: rgba(255, 255, 255, 0.2);\n            border-color: rgba(255, 255, 255, 0.3);\n        }\n\n        .hero-trust {\n            display: flex;\n            gap: 40px;\n            margin-top: 48px;\n            padding-top: 32px;\n            border-top: 1px solid rgba(255, 255, 255, 0.1);\n        }\n\n        .hero-trust-item {\n            display: flex;\n            align-items: center;\n            gap: 12px;\n        }\n\n        .hero-trust-icon {\n            width: 44px;\n            height: 44px;\n            background: rgba(255, 255, 255, 0.1);\n            border-radius: var(--radius-md);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n        }\n\n        .hero-trust-icon svg {\n            width: 22px;\n            height: 22px;\n            color: var(--color-accent);\n        }\n\n        .hero-trust-text {\n            font-weight: 600;\n            color: white;\n        }\n\n        .hero-trust-sub {\n            font-size: 0.8rem;\n            color: rgba(255, 255, 255, 0.6);\n        }\n\n        .hero-visual {\n            position: relative;\n            opacity: 0;\n            transform: translateX(30px);\n            animation: fade-left 0.8s ease 0.2s forwards;\n        }\n\n        @keyframes fade-left {\n            to {\n                opacity: 1;\n                transform: translateX(0);\n            }\n        }\n\n        .hero-image-wrapper {\n            position: relative;\n            border-radius: var(--radius-xl);\n            overflow: hidden;\n        }\n\n        .hero-image {\n            width: 100%;\n            height: 550px;\n            object-fit: cover;\n            display: block;\n        }\n\n        .hero-overlay {\n            position: absolute;\n            bottom: 0;\n            left: 0;\n            right: 0;\n            background: linear-gradient(transparent, rgba(15, 36, 64, 0.95));\n            padding: 40px;\n        }\n\n        .hero-overlay-badge {\n            display: inline-flex;\n            align-items: center;\n            gap: 8px;\n            background: var(--gradient-emergency);\n            padding: 8px 16px;\n            border-radius: var(--radius-full);\n            font-weight: 700;\n            font-size: 0.85rem;\n            margin-bottom: 12px;\n            color: white;\n        }\n\n        .hero-overlay-title {\n            font-family: 'Space Grotesk', sans-serif;\n            font-size: 1.5rem;\n            font-weight: 700;\n            color: white;\n            margin-bottom: 8px;\n        }\n\n        .hero-overlay-text {\n            color: rgba(255, 255, 255, 0.8);\n            font-size: 0.95rem;\n        }\n\n        .hero-float-card {\n            position: absolute;\n            background: white;\n            border-radius: var(--radius-lg);\n            padding: 24px;\n            box-shadow: var(--shadow-xl);\n            animation: float-card 6s ease-in-out infinite;\n        }\n\n        .hero-float-card-1 {\n            top: 40px;\n            left: -40px;\n        }\n\n        .hero-float-card-2 {\n            bottom: 80px;\n            right: -40px;\n            animation-delay: -3s;\n        }\n\n        @keyframes float-card {\n            0%, 100% { transform: translateY(0); }\n            50% { transform: translateY(-20px); }\n        }\n\n        .float-card-header {\n            display: flex;\n            align-items: center;\n            gap: 16px;\n            margin-bottom: 12px;\n        }\n\n        .float-card-icon {\n            width: 56px;\n            height: 56px;\n            background: var(--gradient-emergency);\n            border-radius: var(--radius-md);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            color: white;\n        }\n\n        .float-card-icon svg {\n            width: 28px;\n            height: 28px;\n        }\n\n        .float-card-content h4 {\n            font-family: 'Space Grotesk', sans-serif;\n            font-size: 0.9rem;\n            font-weight: 700;\n            color: var(--color-primary);\n            margin-bottom: 4px;\n        }\n\n        .float-card-content p {\n            font-size: 0.8rem;\n            color: var(--color-gray);\n        }\n\n        .float-card-value {\n            font-size: 2rem;\n            font-weight: 800;\n            color: var(--color-primary);\n            font-family: 'Space Grotesk', sans-serif;\n        }\n\n        /* Emergency Bar */\n        .emergency-bar {\n            background: var(--gradient-emergency);\n            padding: 16px 0;\n            position: relative;\n            overflow: hidden;\n        }\n\n        .emergency-bar-inner {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            gap: 60px;\n        }\n\n        .emergency-bar-item {\n            display: flex;\n            align-items: center;\n            gap: 12px;\n            color: white;\n        }\n\n        .emergency-bar-item svg {\n            width: 28px;\n            height: 28px;\n        }\n\n        .emergency-bar-text {\n            font-weight: 600;\n        }\n\n        .emergency-bar-sub {\n            font-size: 0.85rem;\n            opacity: 0.9;\n        }\n\n        /* Problem Solution Section */\n        .problem-solution {\n            padding: 120px 0;\n            background: white;\n        }\n\n        .section-header {\n            text-align: center;\n            max-width: 800px;\n            margin: 0 auto 80px;\n        }\n\n        .section-label {\n            display: inline-block;\n            font-size: 0.8rem;\n            font-weight: 700;\n            color: var(--color-emergency);\n            text-transform: uppercase;\n            letter-spacing: 2px;\n            margin-bottom: 20px;\n            background: rgba(220, 38, 38, 0.1);\n            padding: 8px 20px;\n            border-radius: var(--radius-full);\n        }\n\n        .section-title {\n            font-family: 'Space Grotesk', sans-serif;\n            font-size: clamp(2rem, 4.5vw, 3.25rem);\n            font-weight: 700;\n            color: var(--color-primary);\n            margin-bottom: 24px;\n            letter-spacing: -0.02em;\n        }\n\n      