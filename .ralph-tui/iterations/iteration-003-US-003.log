       .hero-trust {\n            display: flex;\n            gap: 40px;\n            margin-top: 48px;\n            padding-top: 32px;\n            border-top: 1px solid rgba(255, 255, 255, 0.1);\n        }\n\n        .hero-trust-item {\n            display: flex;\n            align-items: center;\n            gap: 12px;\n        }\n\n        .hero-trust-icon {\n            width: 44px;\n            height: 44px;\n            background: rgba(255, 255, 255, 0.1);\n            border-radius: var(--radius-md);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n        }\n\n        .hero-trust-icon svg {\n            width: 22px;\n            height: 22px;\n            color: var(--color-accent);\n        }\n\n        .hero-trust-text {\n            font-weight: 600;\n            color: white;\n        }\n\n        .hero-trust-sub {\n            font-size: 0.8rem;\n            color: rgba(255, 255, 255, 0.6);\n        }\n\n        .hero-visual {\n            position: relative;\n            opacity: 0;\n            transform: translateX(30px);\n            animation: fade-left 0.8s ease 0.2s forwards;\n        }\n\n        @keyframes fade-left {\n            to {\n                opacity: 1;\n                transform: translateX(0);\n            }\n        }\n\n        .hero-image-wrapper {\n            position: relative;\n            border-radius: var(--radius-xl);\n            overflow: hidden;\n        }\n\n        .hero-image {\n            width: 100%;\n            height: 550px;\n            object-fit: cover;\n            display: block;\n        }\n\n        .hero-overlay {\n            position: absolute;\n            bottom: 0;\n            left: 0;\n            right: 0;\n            background: linear-gradient(transparent, rgba(15, 36, 64, 0.95));\n            padding: 40px;\n        }\n\n        .hero-overlay-badge {\n            display: inline-flex;\n            align-items: center;\n            gap: 8px;\n            background: var(--gradient-emergency);\n            padding: 8px 16px;\n            border-radius: var(--radius-full);\n            font-weight: 700;\n            font-size: 0.85rem;\n            margin-bottom: 12px;\n            color: white;\n        }\n\n        .hero-overlay-title {\n            font-family: 'Space Grotesk', sans-serif;\n            font-size: 1.5rem;\n            font-weight: 700;\n            color: white;\n            margin-bottom: 8px;\n        }\n\n        .hero-overlay-text {\n            color: rgba(255, 255, 255, 0.8);\n            font-size: 0.95rem;\n        }\n\n        .hero-float-card {\n            position: absolute;\n            background: white;\n            border-radius: var(--radius-lg);\n            padding: 24px;\n            box-shadow: var(--shadow-xl);\n            animation: float-card 6s ease-in-out infinite;\n        }\n\n        .hero-float-card-1 {\n            top: 40px;\n            left: -40px;\n        }\n\n        .hero-float-card-2 {\n            bottom: 80px;\n            right: -40px;\n            animation-delay: -3s;\n        }\n\n        @keyframes float-card {\n            0%, 100% { transform: translateY(0); }\n            50% { transform: translateY(-20px); }\n        }\n\n        .float-card-header {\n            display: flex;\n            align-items: center;\n            gap: 16px;\n            margin-bottom: 12px;\n        }\n\n        .float-card-icon {\n            width: 56px;\n            height: 56px;\n            background: var(--gradient-emergency);\n            border-radius: var(--radius-md);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            color: white;\n        }\n\n        .float-card-icon svg {\n            width: 28px;\n            height: 28px;\n        }\n\n        .float-card-content h4 {\n            font-family: 'Space Grotesk', sans-serif;\n            font-size: 0.9rem;\n            font-weigh