→        const contactForm = document.getElementById('contactForm');\n  1760→        contactForm.addEventListener('submit', (e) => {\n  1761→            e.preventDefault();\n  1762→            const formData = new FormData(contactForm);\n  1763→            const data = Object.fromEntries(formData);\n  1764→\n  1765→            // Here you would typically send the data to your backend\n  1766→            // For now, we'll show a success message\n  1767→            alert('Mensagem enviada com sucesso! Entraremos em contato em breve.');\n  1768→            contactForm.reset();\n  1769→        });\n  1770→\n  1771→        // Counter animation for stats\n  1772→        const animateCounter = (element, target, duration = 2000) => {\n  1773→            let start = 0;\n  1774→            const increment = target / (duration / 16);\n  1775→            const timer = setInterval(() => {\n  1776→                start += increment;\n  1777→                if (start >= target) {\n  1778→                    element.textContent = '+' + target;\n  1779→                    clearInterval(timer);\n  1780→                } else {\n  1781→                    element.textContent = '+' + Math.floor(start);\n  1782→                }\n  1783→            }, 16);\n  1784→        };\n  1785→\n  1786→        // Trigger counter animation when hero stat is visible\n  1787→        const heroStat = document.querySelector('.hero-stat-number');\n  1788→        if (heroStat) {\n  1789→            const statObserver = new IntersectionObserver((entries) => {\n  1790→                entries.forEach(entry => {\n  1791→                    if (entry.isIntersecting) {\n  1792→                        animateCounter(entry.target, 500);\n  1793→                        statObserver.unobserve(entry.target);\n  1794→                    }\n  1795→                });\n  1796→            }, { threshold: 0.5 });\n  1797→            statObserver.observe(heroStat);\n  1798→        }\n  1799→    </script>\n  1800→</body>\