flex-direction: column;\n            gap: 6px;\n            cursor: pointer;\n        }\n\n        .menu-toggle span {\n            width: 28px;\n            height: 2px;\n            background: var(--color-primary);\n            transition: var(--transition-fast);\n        }\n\n        /* Hero Section */\n        .hero {\n            min-height: 100vh;\n            display: flex;\n            align-items: center;\n            position: relative;\n            padding: var(--spacing-xl) 0;\n            overflow: hidden;\n        }\n\n        .hero-bg {\n            position: absolute;\n            top: 0;\n            left: 0;\n            width: 100%;\n            height: 100%;\n            z-index: -1;\n        }\n\n        .hero-blob {\n            position: absolute;\n            border-radius: var(--radius-blob);\n            filter: blur(80px);\n            opacity: 0.5;\n            animation: float 20s ease-in-out infinite;\n        }\n\n        .blob-1 {\n            width: 500px;\n            height: 500px;\n            background: var(--color-sage);\n            top: -100px;\n            right: -100px;\n            animation-delay: 0s;\n        }\n\n        .blob-2 {\n            width: 400px;\n            height: 400px;\n            background: var(--color-accent);\n            bottom: -50px;\n            left: -50px;\n            animation-delay: -5s;\n        }\n\n        .blob-3 {\n            width: 300px;\n            height: 300px;\n            background: var(--color-secondary);\n            top: 50%;\n            left: 30%;\n            animation-delay: -10s;\n        }\n\n        @keyframes float {\n            0%, 100% { transform: translate(0, 0) rotate(0deg); }\n            25% { transform: translate(30px, -30px) rotate(5deg); }\n            50% { transform: translate(-20px, 20px) rotate(-5deg); }\n            75% { transform: translate(20px, 30px) rotate(3deg); }\n        }\n\n        .hero-content {\n            display: grid;\n            