adding: var(--spacing-lg) 0 var(--spacing-md);\n        }\n\n        .footer-grid {\n            display: grid;\n            grid-template-columns: 2fr 1fr 1fr;\n            gap: var(--spacing-lg);\n            margin-bottom: var(--spacing-md);\n        }\n\n        .footer-brand h3 {\n            color: var(--color-white);\n            margin-bottom: var(--spacing-sm);\n        }\n\n        .footer-brand p {\n            color: rgba(255, 255, 255, 0.7);\n        }\n\n        .footer-links h4 {\n            color: var(--color-white);\n            margin-bottom: var(--spacing-sm);\n            font-size: 1.1rem;\n        }\n\n        .footer-links ul li {\n            margin-bottom: 0.5rem;\n        }\n\n        .footer-links a {\n            color: rgba(255, 255, 255, 0.7);\n            transition: var(--transition-fast);\n        }\n\n        .footer-links a:hover {\n            color: var(--color-secondary);\n        }\n\n        .footer-bottom {\n            border-top: 1px solid rgba(255, 255, 255, 0.1);\n            padding-top: var(--spacing-md);\n            text-align: center;\n            color: rgba(255, 255, 255, 0.5);\n            font-size: 0.9rem;\n        }\n\n        /* WhatsApp Float Button */\n        .whatsapp-float {\n            position: fixed;\n            bottom: var(--spacing-md);\n            right: var(--spacing-md);\n            width: 60px;\n            height: 60px;\n            background: #25D366;\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            box-shadow: 0 4px 20px rgba(37, 211, 102, 0.4);\n            z-index: 999;\n            transition: var(--transition-fast);\n        }\n\n        .whatsapp-float:hover {\n            transform: scale(1.1);\n        }\n\n        .whatsapp-float svg {\n            width: 32px;\n            height: 32px;\n            color: var(--color-white);\n        }\n\n        /* Scroll Animations */\n        .wow {\n            opacity: 0;\n            transform: translateY(30px);\n            transition: opacity 0.6s ease, transform 0.6s ease;\n        }\n\n        .wow.animated {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .wow-delay-1 { transition-delay: 0.1s; }\n        .wow-delay-2 { transition-delay: 0.2s; }\n        .wow-delay-3 { transition-delay: 0.3s; }\n        .wow-delay-4 { transition-delay: 0.4s; }\n        .wow-delay-5 { transition-delay: 0.5s; }\n        .wow-delay-6 { transition-delay: 0.6s; }\n\n        /* Responsive */\n        @media (max-width: 768px) {\n            .nav-links {\n                position: fixed;\n                top: 70px;\n                left: 0;\n                right: 0;\n                background: var(--color-white);\n                flex-direction: column;\n                padding: var(--spacing-md);\n                gap: var(--spacing-sm);\n                transform: translateY(-150%);\n                transition: transform var(--transition-base);\n                box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n            }\n\n            .nav-links.active {\n                transform: translateY(0);\n            }\n\n            .menu-toggle {\n                display: flex;\n            }\n\n            .hero-content {\n                grid-template-columns: 1fr;\n                text-align: center;\n            }\n\n            .hero-description {\n                margin-left: auto;\n                margin-right: auto;\n            }\n\n            .hero-cta-group {\n                justify-content: center;\n            }\n\n            .hero-image-wrapper img {\n                height: 350px;\n            }\n\n            .problem-solution-grid,\n            .contact-grid,\n            .footer-grid {\n                grid-template-columns: 1fr;\n            }\n\n            h1 { font-size: 2.5rem; }\n            h2 { font-size: 2rem; }\n        }\n\n        @media (max-width: 480px) {\n            .hero-cta-group {\n                flex-direction: column;\n            }\n\n            .btn {\n                width: 100%;\n                justify-content: center;\n            }\n\n            .services-grid {\n                grid-template-columns: 1fr;\n            }\n        }\n\n        /* Reduced Motion */\n        @media (prefers-reduced-motion: reduce) {\n            *, *::before, *::after {\n                animation-duration: 0.01ms !important;\n                animation-iteration-count: 1 !important;\n                transition-duration: 0.01ms !important;\n            }\n        }\n    </style>\n</head>\n<body>\n    <!-- Navigation -->\n    <nav class=\"nav\" id=\"nav\">\n        <div class=\"container nav-container\">\n            <a href=\"#\" class=\"nav-logo\">Arthemia</a>\n            <div class=\"menu-toggle\" id=\"menuToggle\">\n                <span></span>\n                <span></span>\n                <span></span>\n            </div>\n            <ul class=\"nav-links\" id=\"navLinks\">\n                <li><a href=\"#inicio\" class=\"nav-link\">Início</a></li>\n                <li><a href=\"#servicos\" class=\"nav-link\">Serviços</a></li>\n                <li><a href=\"#diferenciais\" class=\"nav-link\">Diferenciais</a></li>\n                <li><a href=\"#depoimentos\" class=\"nav-link\">Depoimentos</a></li>\n                <li><a href=\"#contato\" class=\"nav-cta\">Agendar Consulta</a></li>\n            </ul>\n        </div>\n    </nav>\n\n    <!-- Hero Section -->\n    <section class=\"hero\" id=\"inicio\">\n        <div class=\"hero-bg\">\n            <div class=\"hero-blob blob-1\"></div>\n            <div class=\"hero-blob blob-2\"></div>\n            <div class=\"hero-blob blob-3\"></div>\n        </div>\n        <div class=\"container hero-content\">\n            <div class=\"hero-text\">\n                <span class=\"hero-tag\">Clínica Veterinária em Ribeirão Preto</span>\n                <h1>Cuidado Veterinário com Excelência e Amor</h1>\n                <p class=\"hero-description\">Na Arthemia, seu pet é tratado como família. Oferecemos atendimento veterinário especializado com carinho e dedicação.</p>\n                <div class=\"hero-cta-group\">\n                    <a href=\"#contato\" class=\"btn btn-primary\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 2v20M2 12h20\"/></svg>\n                        Agendar Consulta\n                    </a>\n                    <a href=\"#servicos\" class=\"btn btn-secondary\">Conhecer Serviços</a>\n                </div>\n            </div>\n            <div class=\"hero-image\">\n                <div class=\"hero-image-wrapper\">\n                    <img src=\"https://images.unsplash.com/photo-1581889470536-467bdbe30cd0?w=800&h=600&fit=crop\" alt=\"Veterinária examinando um cachorro com carinho\">\n                </div>\n                <div class=\"hero-image-decoration\"></div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Problem/Solution Section -->\n    <section class=\"problem-solution\">\n        <div class=\"container\">\n            <div class=\"section-header\">\n                <span class=\"section-tag\">Por que escolher a Arthemia?</span>\n                <h2>Preocupamos-se com o que realmente importa</h2>\n            </div>\n            <div class=\"problem-solution-grid\">\n                <div class=\"problem-card wow\">\n                    <div class=\"card-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z\"/></svg>\n                    </div>\n                    <h3>O Desafio</h3>\n                    <p>Sabemos quanto você se preocupa com a saúde do seu pet. Encontrar uma clínica veterinária que combine excelência técnica com atendimento humanizado pode ser difícil.</p>\n                    <p>Muitas vezes, os tutores se sentem perdidos diante de tantas opções e incertezas sobre o melhor tratamento para seus companheiros.</p>\n                </div>\n                <div class=\"solution-card wow\">\n                    <div class=\"card-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M22 11.08V12a10 10 0 1 1-5.93-9.14\"/><polyline points=\"22 4 12 14.01 9 11.01\"/></svg>\n                    </div>\n                    <h3>Nossa Solução</h3>\n                    <p>Na Arthemia Clínica Veterinária, unimos tecnologia de ponta, equipe especializada e, acima de tudo, muito amor pelos animais.</p>\n                    <p>Oferecemos um ambiente acolhedor onde seu pet recebe o melhor tratamento possível, e você, a tranquilidade de saber que está em boas mãos.</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Services Section -->\n    <section class=\"services\" id=\"servicos\">\n        <div class=\"container\">\n            <div class=\"section-header wow\">\n                <span class=\"section-tag\">Nossos Serviços</span>\n                <h2>Cuidado completo para seu pet</h2>\n                <p>Oferecemos uma gama completa de serviços veterinários para todas as necessidades do seu companheiro.</p>\n            </div>\n            <div class=\"services-grid\">\n                <div class=\"service-card wow wow-delay-1\">\n                    <div class=\"service-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M22 12h-4l-3 9L9 3l-3 9H2\"/></svg>\n                    </div>\n                    <h3>Consultas Clínicas</h3>\n                    <p>Atendimento completo com exame físico detalhado, diagnóstico preciso e plano de tratamento personalizado para cada paciente.</p>\n                </div>\n                <div class=\"service-card wow wow-delay-2\">\n                    <div class=\"service-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><path d=\"M8 14s1.5 2 4 2 4-2 4-2\"/><line x1=\"9\" y1=\"9\" x2=\"9.01\" y2=\"9\"/><line x1=\"15\" y1=\"9\" x2=\"15.01\" y2=\"9\"/></svg>\n                    </div>\n                    <h3>Cirurgias</h3>\n                    <p>Procedimentos cirúrgicos realizados com técnicas avançadas, anestesia segura e acompanhamento pós-operatório dedicado.</p>\n                </div>\n                <div class=\"service-card wow wow-delay-3\">\n                    <div class=\"service-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 2.69l5.74 5.74-5.74 5.74L6.26 8.43z\"/><path d=\"M5 19l5-5 5 5\"/><path d=\"M19 19l-5-5-5 5\"/></svg>\n                    </div>\n                    <h3>Vacinação</h3>\n                    <p>Protocolos vacinais personalizados conforme idade, estilo de vida e região, garantindo a proteção adequada do seu pet.</p>\n                </div>\n                <div class=\"service-card wow wow-delay-4\">\n                    <div class=\"service-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M16 4h2a2 2 0 0 1 2 2v14a2 2 0 0 1-2 2H6a2 2 0 0 1-2-2V6a2 2 0 0 1 2-2h2\"/><rect x=\"8\" y=\"2\" width=\"8\" height=\"4\" rx=\"1\" ry=\"1\"/></svg>\n                    </div>\n                    <h3>Exames Laboratoriais</h3>\n                    <p>Análises clínicas completas com equipamentos modernos e resultados rápidos para diagnóstico preciso.</p>\n                </div>\n                <div class=\"service-card wow wow-delay-5\">\n                    <div class=\"service-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 20.94c1.61 0 3.12-.53 4.36-1.43l-4.36-7.51-4.36 7.51c1.24.9 2.75 1.43 4.36 1.43z\"/><path d=\"M16.36 19.51c2.39-1.74 3.64-4.74 3.09-7.75L12 12l-7.45 4.76c-.55 3.01.7 6.01 3.09 7.75l4.36-7.51 4.36 7.51z\"/></svg>\n                    </div>\n                    <h3>Dermatologia</h3>\n                    <p>Tratamento especializado para alergias, infecções de pele e outras dermatologias comuns em cães e gatos.</p>\n                </div>\n                <div class=\"service-card wow wow-delay-6\">\n                    <div class=\"service-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M20.84 4.61a5.5 5.5 0 0 0-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 0 0-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 0 0 0-7.78z\"/></svg>\n                    </div>\n                    <h3>Odontologia</h3>\n                    <p>Cuidados completos com a saúde bucal: limpeza, tártaro, extrações e prevenção de doenças periodontais.</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Differentials Section -->\n    <section class=\"differentials\" id=\"diferenciais\">\n        <div class=\"container\">\n            <div class=\"section-header wow\">\n                <span class=\"section-tag\" style=\"background: rgba(255,255,255,0.2); color: white;\">Nossos Diferenciais</span>\n                <h2>Por que nos escolher?</h2>\n                <p>Compromisso com a excelência e o bem-estar do seu pet.</p>\n            </div>\n            <div class=\"differentials-grid\">\n                <div class=\"differential-item wow wow-delay-1\">\n                    <div class=\"differential-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M20.21 8.89l-3.07-3.07a4.96 4.96 0 0 0-7.01 0l-3.07 3.07a4.96 4.96 0 0 0 0 7.01l3.07 3.07a4.96 4.96 0 0 0 7.01 0l3.07-3.07a4.96 4.96 0 0 0 0-7.01z\"/><path d=\"M14.5 9.5l-5 5\"/></svg>\n                    </div>\n                    <h3>Equipe Especializada</h3>\n                    <p>Veterinários com residência e especializações em diferentes áreas.</p>\n                </div>\n                <div class=\"differential-item wow wow-delay-2\">\n                    <div class=\"differential-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z\"/></svg>\n                    </div>\n                    <h3>Equipamentos Modernos</h3>\n                    <p>Tecnologia de ponta para diagnósticos precisos e tratamentos eficazes.</p>\n                </div>\n                <div class=\"differential-item wow wow-delay-3\">\n                    <div class=\"differential-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><path d=\"M12 6v6l4 2\"/></svg>\n                    </div>\n                    <h3>Atendimento Ágil</h3>\n                    <p>Horários flexíveis e pronto atendimento para emergências.</p>\n                </div>\n                <div class=\"differential-item wow wow-delay-4\">\n                    <div class=\"differential-icon\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M20.84 4.61a5.5 5.5 0 0 0-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 0 0-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 0 0 0-7.78z\"/></svg>\n                    </div>\n                    <h3>Atendimento Humanizado</h3>\n                    <p>Cuidado com carinho e respeito, tratando cada pet como único.</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Testimonials Section -->\n    <section class=\"testimonials\" id=\"depoimentos\">\n        <div class=\"container\">\n            <div class=\"section-header wow\">\n                <span class=\"section-tag\">Depoimentos</span>\n                <h2>O que nossos clientes dizem</h2>\n                <p>A confiança de quem já confia a nós o bem mais precioso.</p>\n            </div>\n            <div class=\"testimonials-grid\">\n                <div class=\"testimonial-card wow wow-delay-1\">\n                    <div class=\"testimonial-rating\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                    </div>\n                    <p class=\"testimonial-text\">\"A Dra. Marcela salvou a vida do meu gatinho. O atendimento foi impecável desde a recepção até o acompanhamento pós-cirúrgico. Recomendo de olhos fechados!\"</p>\n                    <div class=\"testimonial-author\">\n                        <div class=\"testimonial-avatar\">MC</div>\n                        <div class=\"testimonial-info\">\n                            <h4>Mariana Costa</h4>\n                            <span>Tutora do Mingau</span>\n                        </div>\n                    </div>\n                    <span class=\"testimonial-quote\">\"</span>\n                </div>\n                <div class=\"testimonial-card wow wow-delay-2\">\n                    <div class=\"testimonial-rating\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><polygon points=\"12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2\"/></svg>\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 2