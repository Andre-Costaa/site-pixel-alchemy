ease;\n\n            --container: 1200px;\n            --header-height: 80px;\n        }\n\n        html {\n            scroll-behavior: smooth;\n        }\n\n        body {\n            font-family: var(--font-body);\n            color: var(--color-charcoal);\n            background-color: var(--color-cream);\n            line-height: 1.6;\n            overflow-x: hidden;\n        }\n\n        .container {\n            max-width: var(--container);\n            margin: 0 auto;\n            padding: 0 20px;\n        }\n\n        /* Header */\n        .header {\n            position: fixed;\n            top: 0;\n            left: 0;\n            right: 0;\n            height: var(--header-height);\n            background: rgba(254, 247, 237, 0.95);\n            backdrop-filter: blur(20px);\n            z-index: 1000;\n            border-bottom: 1px solid rgba(245, 158, 11, 0.1);\n            transition: var(--transition-base);\n        }\n\n        .header.scrolled {\n            box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);\n        }\n\n        .header-inner {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            height: 100%;\n        }\n\n        .logo {\n            font-family: var(--font-display);\n            font-weight: 800;\n            font-size: 1.75rem;\n            color: var(--color-charcoal);\n            text-decoration: none;\n            display: flex;\n            align-items: center;\n            gap: 10px;\n        }\n\n        .logo-icon {\n            width: 45px;\n            height: 45px;\n            background: linear-gradient(135deg, var(--color-amber-primary), var(--color-amber-dark));\n            border-radius: 12px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n        }\n\n        .logo-icon svg {\n            width: 28px;\n            height: 28px;\n            fill: white;\n        }\n\n        .logo-text {\n       