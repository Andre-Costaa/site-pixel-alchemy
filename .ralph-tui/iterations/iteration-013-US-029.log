22→\n  1923→        // --- Mobile Menu Toggle ---\n  1924→        const mobileToggle = document.getElementById('mobileToggle');\n  1925→        const navList = document.getElementById('navList');\n  1926→\n  1927→        mobileToggle.addEventListener('click', () => {\n  1928→            navList.classList.toggle('active');\n  1929→        });\n  1930→\n  1931→        // Close mobile menu when clicking a link\n  1932→        document.querySelectorAll('.nav-link').forEach(link => {\n  1933→            link.addEventListener('click', () => {\n  1934→                navList.classList.remove('active');\n  1935→            });\n  1936→        });\n  1937→\n  1938→        // --- Smooth Scroll ---\n  1939→        document.querySelectorAll('a[href^=\"#\"]').forEach(anchor => {\n  1940→            anchor.addEventListener('click', function (e) {\n  1941→                e.preventDefault();\n  1942→                const target = document.querySelector(this.getAttribute('href'));\n  1943→                if (target) {\n  1944→                    const headerOffset = 80;\n  1945→                    const elementPosition = target.getBoundingClientRect().top;\n  1946→                    const offsetPosition = elementPosition + window.pageYOffset - headerOffset;\n  1947→\n  1948→                    window.scrollTo({\n  1949→                        top: offsetPosition,\n  1950→                        behavior: 'smooth'\n  1951→                    });\n  1952→                }\n  1953→            });\n  1954→        });\n  1955→\n  1956→        // --- Intersection Observer for Animations ---\n  1957→        const observerOptions = {\n  1958→            root: null,\n  1959→            rootMargin: '0px',\n  1960→            threshold: 0.1\n  1961→        };\n  1962→\n  1963→        const observer = new IntersectionObserver((entries) => {\n  1964→            entries.forEach(entry => {\n  1965→                if (entry.isIntersecting) {\n  1966→                    entry.target.classList.add('animated');\n  1967→\n  1968→                    // Handle delay attribute\n  1969→                    const delay = entry.target.getAttribute('data-delay');\n  1970→                    if (delay) {\n  1971→                        entry.target.style.setProperty('--delay', delay + 'ms');\n  1972→                    }\n  1973→\n  1974→                    observer.unobserve(entry.target);\n  1975→                }\n  1976→            });\n  1977→        }, observerOptions);\n  1978→\n  1979→        // Observe all animated elements\n  1980→        document.querySelectorAll('.wow-fade-up, .wow-fade-in').forEach(el => {\n  1981→            observer.observe(el);\n  1982→        });\n  1983→\n  1984→        // --- Form Handling ---\n  1985→        const contactForm = document.getElementById('contactForm');\n  1986→\n  1987→        contactForm.addEventListener('submit', (e) => {\n  1988→            e.preventDefault();\n  1989→\n  1990→            const formData = new FormData(contactForm);\n  1991→            const data = Object.fromEntries(formData.entries());\n  1992→\n  1993→            // Log the form data (in production, send to backend)\n  1994→            console.log('Form submitted:', data);\n  1995→\n  1996→            // Show success message\n  1997→            alert('Obrigado pelo contato! Em breve entraremos em contato.');\n  1998→            contactForm.reset();\n  1999→        });\n  2000→\n  2001→        // --- Phone Number Click to Call ---\n  2002→        document.querySelectorAll('a[href^=\"tel:\"]').forEach(link => {\n  2003→            link.addEventListener('click', (e) => {\n  2004→                // Analytics tracking could go here\n  2005→                console.log('Phone call initiated');\n  2006→            });\n  2007→        });\n  2008→\n  2009→        // --- Emergency Button Pulse Animation ---\n  2010→        const emergencyBtn = document.querySelector('.nav-cta');\n  2011→        if (emergencyBtn) {\n  2012→            // Enhance visibility on scroll\n  2013→            window.addEventListener('scroll', () => {\n  2014→                if (window.pageYOffset > 500) {\n  2015→                    emergencyBtn.style.animation = 'heartbeat 1s infinite';\n  2016→                } else {\n  2017→                    emergencyBtn.style.animation = 'heartbeat 2s infinite';\n  2018→                }\n  2019→            });\n  2020→        }\n  2021→\n  2022→        // --- Lazy Loading Images ---\n  2023→        if ('loading' in HTMLImageElement.prototype) {\n  2024→            const images = document.querySelectorAll('img[loading=\"lazy\"]');\n  2025→            images.forEach(img => {\n  2026→                img.src = img.dataset.src || img.src;\n  2027→            });\n  2028→        }\n  2029→\n  2030→        // --- Console Welcome Message ---\n  2031→        console.log('%c🐾 Dog Days - Clínica Veterinária 24h', 'font-size: 20px; font-weight: bold; color: #0d9488;');\n  2032→        console.log('%cAtendimento 24h em Ribeirão Preto', 'font-size: 14px; color: #64748b;');\n  2033→        console.log('%c📞 (16) 99147-8768', 'font-size: 14px; color: #f97316;');\n  2034→    </script>\n  2035→</body>\n  2036→</html>\n  2037→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"38d282f1-6e85-4d3e-8558-8cec8874b87e","uuid":"c8247ae9-ba17-4854-ba3b-bd494648fe1c","tool_use_result":{"type":"text","file":{"filePath":"/home/ac/Projetos/site-pixel-alchemy/site-pixel-alchemy/site-demo/dog-days-clinica-veterinaria-24h/index.html","content":"<!DOCTYPE html>\n<html lang=\"pt-BR\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Dog Days - Clínica Veterinária 24h | Ribeirão Preto</title>\n    <meta name=\"description\" content=\"Dog Days - Clínica Veterinária 24h em Ribeirão Preto. Atendimento emergencial 24 horas para seu pet. Equipe especializada, equipamentos modernos.\">\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n    <link href=\"https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@400;500;600;700&family=Inter:wght@400;500;600&display=swap\" rel=\"stylesheet\">\n    <style>\n        /* ============================================\n           DOG DAYS - CLÍNICA VETERINÁRIA 24H\n           Design System & Styles\n           ============================================ */\n\n        /* --- CSS Variables --- */\n        :root {\n            /* Colors - Emergency/Medical Palette */\n            --color-primary: #0d9488;\n            --color-primary-dark: #0f766e;\n            --color-primary-light: #14b8a6;\n            --color-accent: #f97316;\n            --color-accent-light: #fb923c;\n            --color-dark: #1e293b;\n            --color-darker: #0f172a;\n            --color-light: #f8fafc;\n            --color-white: #ffffff;\n            --color-gray: #64748b;\n            --color-gray-light: #e2e8f0;\n            --color-danger: #ef4444;\n            --color-danger-light: #fca5a5;\n\n            /* Gradients */\n            --gradient-primary: linear-gradient(135deg, var(--color-primary) 0%, var(--color-primary-dark) 100%);\n            --gradient-accent: linear-gradient(135deg, var(--color-accent) 0%, var(--color-accent-light) 100%);\n            --gradient-dark: linear-gradient(180deg, var(--color-darker) 0%, var(--color-dark) 100%);\n            --gradient-hero: linear-gradient(135deg, rgba(13, 148, 136, 0.95) 0%, rgba(15, 118, 110, 0.9) 50%, rgba(30, 41, 59, 0.95) 100%);\n\n            /* Typography */\n            --font-display: 'Space Grotesk', sans-serif;\n            --font-body: 'Inter', sans-serif;\n\n            /* Spacing */\n            --spacing-xs: 0.5rem;\n            --spacing-sm: 1rem;\n            --spacing-md: 2rem;\n            --spacing-lg: 4rem;\n            --spacing-xl: 6rem;\n\n            /* Border Radius */\n            --radius-sm: 8px;\n            --radius-md: 16px;\n            --radius-lg: 24px;\n            --radius-xl: 40px;\n            --radius-blob: 60% 40% 30% 70% / 60% 30% 70% 40%;\n\n            /* Transitions */\n            --transition-fast: 0.2s ease;\n            --transition-base: 0.4s ease;\n            --transition-slow: 0.6s ease;\n\n            /* Shadows */\n            --shadow-sm: 0 2px 8px rgba(0, 0, 0, 0.08);\n            --shadow-md: 0 8px 24px rgba(0, 0, 0, 0.12);\n            --shadow-lg: 0 16px 48px rgba(0, 0, 0, 0.16);\n            --shadow-glow: 0 0 40px rgba(13, 148, 136, 0.3);\n\n            /* Z-index */\n            --z-base: 1;\n            --z-elevated: 10;\n            --z-header: 100;\n            --z-modal: 1000;\n        }\n\n        /* --- Reset & Base --- */\n        *, *::before, *::after {\n            margin: 0;\n            padding: 0;\n            box-sizing: border-box;\n        }\n\n        html {\n            scroll-behavior: smooth;\n        }\n\n        body {\n            font-family: var(--font-body);\n            font-size: 16px;\n            line-height: 1.6;\n            color: var(--color-dark);\n            background: var(--color-light);\n            overflow-x: hidden;\n        }\n\n        /* --- Typography --- */\n        h1, h2, h3, h4, h5, h6 {\n            font-family: var(--font-display);\n            font-weight: 700;\n            line-height: 1.2;\n        }\n\n        h1 { font-size: clamp(2.5rem, 6vw, 4.5rem); }\n        h2 { font-size: clamp(2rem, 4vw, 3rem); }\n        h3 { font-size: clamp(1.25rem, 2.5vw, 1.75rem); }\n\n        p {\n            margin-bottom: 1rem;\n        }\n\n        a {\n            color: inherit;\n            text-decoration: none;\n            transition: color var(--transition-fast);\n        }\n\n        img {\n            max-width: 100%;\n            height: auto;\n            display: block;\n        }\n\n        /* --- Utilities --- */\n        .container {\n            width: 100%;\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 0 var(--spacing-md);\n        }\n\n        .section {\n            padding: var(--spacing-xl) 0;\n            position: relative;\n        }\n\n        .text-center { text-align: center; }\n        .text-primary { color: var(--color-primary); }\n        .text-accent { color: var(--color-accent); }\n\n        .btn {\n            display: inline-flex;\n            align-items: center;\n            justify-content: center;\n            gap: var(--spacing-xs);\n            padding: 1rem 2rem;\n            font-family: var(--font-display);\n            font-weight: 600;\n            font-size: 1rem;\n            border-radius: var(--radius-xl);\n            border: none;\n            cursor: pointer;\n            transition: all var(--transition-base);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .btn-primary {\n            background: var(--gradient-accent);\n            color: var(--color-white);\n            box-shadow: 0 4px 20px rgba(249, 115, 22, 0.4);\n        }\n\n        .btn-primary:hover {\n            transform: translateY(-2px);\n            box-shadow: 0 8px 30px rgba(249, 115, 22, 0.5);\n        }\n\n        .btn-secondary {\n            background: var(--color-white);\n            color: var(--color-primary);\n            border: 2px solid var(--color-primary);\n        }\n\n        .btn-secondary:hover {\n            background: var(--color-primary);\n            color: var(--color-white);\n        }\n\n        .btn-large {\n            padding: 1.25rem 2.5rem;\n            font-size: 1.125rem;\n        }\n\n        /* --- Animations --- */\n        @keyframes fadeInUp {\n            from {\n                opacity: 0;\n                transform: translateY(30px);\n            }\n            to {\n                opacity: 1;\n                transform: translateY(0);\n            }\n        }\n\n        @keyframes fadeIn {\n            from { opacity: 0; }\n            to { opacity: 1; }\n        }\n\n        @keyframes pulse-ring {\n            0% {\n                transform: scale(1);\n                opacity: 1;\n            }\n            100% {\n                transform: scale(1.5);\n                opacity: 0;\n            }\n        }\n\n        @keyframes float {\n            0%, 100% { transform: translateY(0); }\n            50% { transform: translateY(-20px); }\n        }\n\n        @keyframes heartbeat {\n            0%, 100% { transform: scale(1); }\n            25% { transform: scale(1.05); }\n            50% { transform: scale(1); }\n            75% { transform: scale(1.05); }\n        }\n\n        .wow-fade-up {\n            opacity: 0;\n            transform: translateY(30px);\n            transition: all 0.6s ease;\n        }\n\n        .wow-fade-up.animated {\n            opacity: 1;\n            transform: translateY(0);\n        }\n\n        .wow-fade-in {\n            opacity: 0;\n            transition: opacity 0.6s ease;\n        }\n\n        .wow-fade-in.animated {\n            opacity: 1;\n        }\n\n        [data-delay] {\n            transition-delay: var(--delay);\n        }\n\n        /* --- Header & Navigation --- */\n        .header {\n            position: fixed;\n            top: 0;\n            left: 0;\n            right: 0;\n            z-index: var(--z-header);\n            padding: 1rem 0;\n            transition: all var(--transition-base);\n        }\n\n        .header.scrolled {\n            background: rgba(255, 255, 255, 0.98);\n            backdrop-filter: blur(10px);\n            box-shadow: var(--shadow-sm);\n            padding: 0.75rem 0;\n        }\n\n        .header.scrolled .logo-text,\n        .header.scrolled .nav-link {\n            color: var(--color-dark);\n        }\n\n        .header.scrolled .nav-link::after {\n            background: var(--color-primary);\n        }\n\n        .header-content {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n        }\n\n        .logo {\n            display: flex;\n            align-items: center;\n            gap: 0.75rem;\n        }\n\n        .logo-icon {\n            width: 50px;\n            height: 50px;\n            background: var(--color-white);\n            border-radius: var(--radius-md);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n        }\n\n        .logo-text {\n            font-family: var(--font-display);\n            font-size: 1.5rem;\n            font-weight: 700;\n            color: var(--color-white);\n        }\n\n        .logo-text span {\n            color: var(--color-accent);\n        }\n\n        .nav {\n            display: flex;\n            align-items: center;\n            gap: 2rem;\n        }\n\n        .nav-list {\n            display: flex;\n            align-items: center;\n            gap: 2rem;\n            list-style: none;\n        }\n\n        .nav-link {\n            color: var(--color-white);\n            font-weight: 500;\n            position: relative;\n            padding: 0.5rem 0;\n        }\n\n        .nav-link::after {\n            content: '';\n            position: absolute;\n            bottom: 0;\n            left: 0;\n            width: 0;\n            height: 2px;\n            background: var(--color-accent);\n            transition: width var(--transition-fast);\n        }\n\n        .nav-link:hover::after {\n            width: 100%;\n        }\n\n        .nav-cta {\n            display: flex;\n            align-items: center;\n            gap: 0.5rem;\n            background: var(--color-white);\n            color: var(--color-primary);\n            padding: 0.625rem 1.25rem;\n            border-radius: var(--radius-xl);\n            font-weight: 600;\n            font-size: 0.875rem;\n            animation: heartbeat 2s infinite;\n        }\n\n        .nav-cta svg {\n            width: 18px;\n            height: 18px;\n            color: var(--color-danger);\n        }\n\n        .mobile-toggle {\n            display: none;\n            flex-direction: column;\n            gap: 6px;\n            background: none;\n            border: none;\n            cursor: pointer;\n            padding: 0.5rem;\n        }\n\n        .mobile-toggle span {\n            width: 24px;\n            height: 2px;\n            background: var(--color-white);\n            border-radius: 2px;\n            transition: all var(--transition-fast);\n        }\n\n        .header.scrolled .mobile-toggle span {\n            background: var(--color-dark);\n        }\n\n        /* --- Hero Section --- */\n        .hero {\n            min-height: 100vh;\n            background: var(--gradient-hero);\n            position: relative;\n            display: flex;\n            align-items: center;\n            overflow: hidden;\n        }\n\n        .hero::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            bottom: 0;\n            background: url('https://images.unsplash.com/photo-1583337130417-3346a1be7dee?w=1920&q=80') center/cover;\n            opacity: 0.15;\n        }\n\n        .hero-blob {\n            position: absolute;\n            border-radius: var(--radius-blob);\n            filter: blur(60px);\n            opacity: 0.3;\n        }\n\n        .hero-blob-1 {\n            width: 500px;\n            height: 500px;\n            background: var(--color-accent);\n            top: -200px;\n            right: -100px;\n            animation: float 8s ease-in-out infinite;\n        }\n\n        .hero-blob-2 {\n            width: 400px;\n            height: 400px;\n            background: var(--color-primary-light);\n            bottom: -150px;\n            left: -100px;\n            animation: float 10s ease-in-out infinite reverse;\n        }\n\n        .hero-content {\n            position: relative;\n            z-index: 2;\n            display: grid;\n            grid-template-columns: 1fr 1fr;\n            gap: var(--spacing-lg);\n            align-items: center;\n        }\n\n        .hero-text {\n            color: var(--color-white);\n        }\n\n        .hero-badge {\n            display: inline-flex;\n            align-items: center;\n            gap: 0.5rem;\n            background: rgba(249, 115, 22, 0.2);\n            color: var(--color-accent-light);\n            padding: 0.5rem 1rem;\n            border-radius: var(--radius-xl);\n            font-size: 0.875rem;\n            font-weight: 600;\n            margin-bottom: var(--spacing-md);\n            border: 1px solid rgba(249, 115, 22, 0.3);\n        }\n\n        .hero-badge .dot {\n            width: 8px;\n            height: 8px;\n            background: var(--color-accent);\n            border-radius: 50%;\n            animation: pulse 2s infinite;\n        }\n\n        @keyframes pulse {\n            0%, 100% { opacity: 1; }\n            50% { opacity: 0.5; }\n        }\n\n        .hero-title {\n            color: var(--color-white);\n            margin-bottom: var(--spacing-md);\n        }\n\n        .hero-title span {\n            color: var(--color-accent-light);\n        }\n\n        .hero-description {\n            font-size: 1.125rem;\n            opacity: 0.9;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .hero-actions {\n            display: flex;\n            gap: 1rem;\n            flex-wrap: wrap;\n        }\n\n        .hero-phone {\n            display: flex;\n            align-items: center;\n            gap: 0.75rem;\n            margin-top: var(--spacing-md);\n            padding: 1rem 1.5rem;\n            background: rgba(255, 255, 255, 0.1);\n            backdrop-filter: blur(10px);\n            border-radius: var(--radius-lg);\n            border: 1px solid rgba(255, 255, 255, 0.2);\n        }\n\n        .hero-phone-icon {\n            width: 50px;\n            height: 50px;\n            background: var(--color-danger);\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            position: relative;\n        }\n\n        .hero-phone-icon::before {\n            content: '';\n            position: absolute;\n            width: 100%;\n            height: 100%;\n            border-radius: 50%;\n            background: var(--color-danger);\n            animation: pulse-ring 2s infinite;\n        }\n\n        .hero-phone-icon svg {\n            width: 24px;\n            height: 24px;\n            color: var(--color-white);\n            position: relative;\n            z-index: 1;\n        }\n\n        .hero-phone-content {\n            color: var(--color-white);\n        }\n\n        .hero-phone-label {\n            font-size: 0.875rem;\n            opacity: 0.8;\n        }\n\n        .hero-phone-number {\n            font-family: var(--font-display);\n            font-size: 1.5rem;\n            font-weight: 700;\n        }\n\n        .hero-image {\n            position: relative;\n        }\n\n        .hero-main-image {\n            width: 100%;\n            border-radius: var(--radius-xl);\n            box-shadow: var(--shadow-glow);\n        }\n\n        .hero-float-card {\n            position: absolute;\n            background: var(--color-white);\n            padding: 1rem 1.5rem;\n            border-radius: var(--radius-lg);\n            box-shadow: var(--shadow-lg);\n            display: flex;\n            align-items: center;\n            gap: 1rem;\n            animation: float 4s ease-in-out infinite;\n        }\n\n        .hero-float-card-1 {\n            bottom: 40px;\n            left: -40px;\n        }\n\n        .hero-float-card-2 {\n            top: 40px;\n            right: -40px;\n            animation-delay: 1s;\n        }\n\n        .float-card-icon {\n            width: 48px;\n            height: 48px;\n            background: var(--gradient-primary);\n            border-radius: var(--radius-md);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            flex-shrink: 0;\n        }\n\n        .float-card-icon svg {\n            width: 24px;\n            height: 24px;\n            color: var(--color-white);\n        }\n\n        .float-card-text {\n            font-weight: 600;\n            color: var(--color-dark);\n        }\n\n        /* --- Problem/Solution Section --- */\n        .problem-solution {\n            background: var(--color-white);\n        }\n\n        .problem-solution-grid {\n            display: grid;\n            grid-template-columns: 1fr 1fr;\n            gap: var(--spacing-lg);\n        }\n\n        .ps-card {\n            padding: var(--spacing-md);\n            border-radius: var(--radius-lg);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .ps-problem {\n            background: linear-gradient(135deg, #fef2f2 0%, #fee2e2 100%);\n        }\n\n        .ps-solution {\n            background: linear-gradient(135deg, #f0fdfa 0%, #ccfbf1 100%);\n        }\n\n        .ps-icon {\n            width: 64px;\n            height: 64px;\n            border-radius: var(--radius-lg);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: var(--spacing-sm);\n        }\n\n        .ps-problem .ps-icon {\n            background: rgba(239, 68, 68, 0.15);\n        }\n\n        .ps-problem .ps-icon svg {\n            color: var(--color-danger);\n        }\n\n        .ps-solution .ps-icon {\n            background: var(--color-primary);\n        }\n\n        .ps-solution .ps-icon svg {\n            color: var(--color-white);\n        }\n\n        .ps-title {\n            margin-bottom: 1rem;\n        }\n\n        .ps-problem .ps-title {\n            color: var(--color-danger);\n        }\n\n        .ps-solution .ps-title {\n            color: var(--color-primary-dark);\n        }\n\n        .ps-list {\n            list-style: none;\n            margin-top: var(--spacing-sm);\n        }\n\n        .ps-list li {\n            display: flex;\n            align-items: flex-start;\n            gap: 0.75rem;\n            margin-bottom: 0.75rem;\n        }\n\n        .ps-list li svg {\n            width: 20px;\n            height: 20px;\n            flex-shrink: 0;\n            margin-top: 2px;\n        }\n\n        .ps-problem .ps-list li svg {\n            color: var(--color-danger);\n        }\n\n        .ps-solution .ps-list li svg {\n            color: var(--color-primary);\n        }\n\n        /* --- Services Section --- */\n        .services {\n            background: var(--color-light);\n        }\n\n        .section-header {\n            text-align: center;\n            max-width: 700px;\n            margin: 0 auto var(--spacing-lg);\n        }\n\n        .section-subtitle {\n            display: inline-block;\n            color: var(--color-primary);\n            font-weight: 600;\n            font-size: 0.875rem;\n            text-transform: uppercase;\n            letter-spacing: 2px;\n            margin-bottom: 0.75rem;\n        }\n\n        .section-title {\n            color: var(--color-darker);\n            margin-bottom: 1rem;\n        }\n\n        .section-description {\n            color: var(--color-gray);\n            font-size: 1.125rem;\n        }\n\n        .services-grid {\n            display: grid;\n            grid-template-columns: repeat(3, 1fr);\n            gap: 1.5rem;\n        }\n\n        .service-card {\n            background: var(--color-white);\n            padding: var(--spacing-md);\n            border-radius: var(--radius-lg);\n            box-shadow: var(--shadow-sm);\n            transition: all var(--transition-base);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .service-card::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            height: 4px;\n            background: var(--gradient-primary);\n            transform: scaleX(0);\n            transform-origin: left;\n            transition: transform var(--transition-base);\n        }\n\n        .service-card:hover {\n            transform: translateY(-8px);\n            box-shadow: var(--shadow-lg);\n        }\n\n        .service-card:hover::before {\n            transform: scaleX(1);\n        }\n\n        .service-icon {\n            width: 72px;\n            height: 72px;\n            background: linear-gradient(135deg, var(--color-primary-light) 0%, var(--color-primary) 100%);\n            border-radius: var(--radius-lg);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: 1.25rem;\n        }\n\n        .service-icon svg {\n            width: 36px;\n            height: 36px;\n            color: var(--color-white);\n        }\n\n        .service-card:nth-child(2) .service-icon {\n            background: var(--gradient-accent);\n        }\n\n        .service-card:nth-child(3) .service-icon {\n            background: linear-gradient(135deg, #8b5cf6 0%, #7c3aed 100%);\n        }\n\n        .service-card:nth-child(4) .service-icon {\n            background: linear-gradient(135deg, #06b6d4 0%, #0891b2 100%);\n        }\n\n        .service-card:nth-child(5) .service-icon {\n            background: linear-gradient(135deg, #ec4899 0%, #db2777 100%);\n        }\n\n        .service-card:nth-child(6) .service-icon {\n            background: