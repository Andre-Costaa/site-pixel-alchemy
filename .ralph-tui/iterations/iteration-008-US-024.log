 width: 36px;\n            height: 36px;\n            fill: var(--color-coral);\n        }\n\n        .nav-links {\n            display: flex;\n            gap: var(--spacing-lg);\n            align-items: center;\n        }\n\n        .nav-link {\n            font-weight: 600;\n            color: var(--color-soft-gray);\n            font-size: 0.9375rem;\n            position: relative;\n            transition: var(--transition-smooth);\n        }\n\n        .nav-link:hover {\n            color: var(--color-coral);\n        }\n\n        .nav-link::after {\n            content: '';\n            position: absolute;\n            bottom: -4px;\n            left: 50%;\n            transform: translateX(-50%) scaleX(0);\n            width: 100%;\n            height: 2px;\n            background: var(--color-coral);\n            transition: var(--transition-smooth);\n        }\n\n        .nav-link:hover::after {\n            transform: translateX(-50%) scaleX(1);\n        }\n\n        .nav-cta {\n            background: var(--gradient-coral);\n            color: white;\n            padding: 0.75rem 1.75rem;\n            border-radius: var(--border-radius-pill);\n            font-weight: 700;\n            font-size: 0.9375rem;\n            transition: var(--transition-smooth);\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-xs);\n            box-shadow: 0 4px 20px rgba(232, 120, 106, 0.3);\n        }\n\n        .nav-cta:hover {\n            transform: translateY(-3px);\n            box-shadow: 0 8px 30px rgba(232, 120, 106, 0.4);\n        }\n\n        .nav-cta svg {\n            width: 18px;\n            height: 18px;\n            fill: white;\n        }\n\n        .mobile-toggle {\n            display: none;\n            flex-direction: column;\n            gap: 5px;\n            cursor: pointer;\n            padding: 5px;\n        }\n\n        .mobile-toggle span {\n            width: 24px;\n            height: 2px;\n            background: var(--color-coral);\n            transition: var(--transition-smooth);\n            border-radius: 2px;\n        }\n\n        /* Hero Section */\n        .hero {\n            min-height: 100vh;\n            display: flex;\n            align-items: center;\n            background: var(--gradient-hero);\n            position: relative;\n            overflow: hidden;\n            padding-top: 80px;\n        }\n\n        .hero-blob {\n            position: absolute;\n            border-radius: var(--border-radius-blob);\n            filter: blur(80px);\n            opacity: 0.35;\n            animation: blob-morph 25s ease-in-out infinite;\n        }\n\n        .blob-1 {\n            width: 600px;\n            height: 600px;\n            background: linear-gradient(135deg, var(--color-coral) 0%, var(--color-sage) 100%);\n            top: -250px;\n            right: -150px;\n            animation-delay: 0s;\n        }\n\n        .blob-2 {\n            width: 500px;\n            height: 500px;\n            background: linear-gradient(135deg, var(--color-sky) 0%, var(--color-sage) 100%);\n            bottom: -200px;\n            left: -150px;\n            animation-delay: -12s;\n        }\n\n        .blob-3 {\n            width: 350px;\n            height: 350px;\n            background: var(--color-sage);\n            top: 40%;\n            left: 10%;\n            opacity: 0.2;\n            animation-delay: -6s;\n        }\n\n        @keyframes blob-morph {\n            0%, 100% {\n                transform: translate(0, 0) rotate(0deg) scale(1);\n                border-radius: 60% 40% 30% 70% / 60% 30% 70% 40%;\n            }\n            25% {\n                transform: translate(40px, -40px) rotate(8deg) scale(1.05);\n                border-radius: 70% 30% 50% 50% / 50% 50% 30% 70%;\n            }\n            50% {\n                transform: translate(-30px, 30px) rotate(-5deg) scale(0.95);\n                border-radius: 40% 60% 70% 30% / 40% 40% 60% 60%;\n            }\n            75% {\n                transform: translate(30px, 40px) rotate(5deg) scale(1.02);\n                border-radius: 50% 50% 30% 70% / 60% 40% 60% 40%;\n            }\n        }\n\n        .hero-container {\n            display: grid;\n            grid-template-columns: 1.1fr 0.9fr;\n            gap: var(--spacing-2xl);\n            align-items: center;\n            position: relative;\n            z-index: 1;\n        }\n\n        .hero-content {\n            padding: var(--spacing-lg) 0;\n        }\n\n        .hero-badge {\n            display: inline-flex;\n            align-items: center;\n            gap: var(--spacing-xs);\n            background: rgba(232, 120, 106, 0.12);\n            color: var(--color-coral-dark);\n            padding: 0.6rem 1.25rem;\n            border-radius: var(--border-radius-pill);\n            font-size: 0.8125rem;\n            font-weight: 700;\n            margin-bottom: var(--spacing-md);\n            text-transform: uppercase;\n            letter-spacing: 0.12em;\n        }\n\n        .hero-badge svg {\n            width: 16px;\n            height: 16px;\n            fill: var(--color-coral);\n            animation: pulse-glow 2.5s ease-in-out infinite;\n        }\n\n        @keyframes pulse-glow {\n            0%, 100% { opacity: 1; transform: scale(1); }\n            50% { opacity: 0.8; transform: scale(1.15); }\n        }\n\n        .hero-title {\n            font-family: var(--font-display);\n            font-size: clamp(2.5rem, 5.5vw, 4.25rem);\n            font-weight: 800;\n            color: var(--color-deep-navy);\n            line-height: 1.1;\n            margin-bottom: var(--spacing-md);\n            letter-spacing: -0.02em;\n        }\n\n        .hero-title .highlight {\n            color: var(--color-coral);\n            position: relative;\n            display: inline-block;\n        }\n\n        .hero-title .highlight::after {\n            content: '';\n            position: absolute;\n            bottom: 8px;\n            left: 0;\n            width: 100%;\n            height: 12px;\n            background: linear-gradient(90deg, rgba(232, 120, 106, 0.3) 0%, rgba(232, 120, 106, 0.1) 100%);\n            z-index: -1;\n            border-radius: 6px;\n        }\n\n        .hero-subtitle {\n            font-size: clamp(1rem, 2vw, 1.1875rem);\n            color: var(--color-soft-gray);\n            margin-bottom: var(--spacing-xl);\n            max-width: 520px;\n            line-height: 1.9;\n        }\n\n        .hero-buttons {\n            display: flex;\n            gap: var(--spacing-md);\n            flex-wrap: wrap;\n        }\n\n        .btn {\n            display: inline-flex;\n            align-items: center;\n            gap: var(--spacing-xs);\n            padding: 1rem 2.25rem;\n            border-radius: var(--border-radius-pill);\n            font-weight: 700;\n            font-size: 0.9375rem;\n            transition: var(--transition-smooth);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .btn svg {\n            width: 18px;\n            height: 18px;\n            transition: var(--transition-smooth);\n        }\n\n        .btn-primary {\n            background: var(--gradient-coral);\n            color: white;\n            box-shadow: var(--shadow-coral);\n        }\n\n        .btn-primary:hover {\n            transform: translateY(-4px);\n            box-shadow: 0 16px 50px rgba(232, 120, 106, 0.35);\n        }\n\n        .btn-primary:hover svg {\n            transform: translateX(5px);\n        }\n\n        .btn-secondary {\n            background: white;\n            color: var(--color-deep-navy);\n            border: 2px solid var(--color-light-gray);\n        }\n\n        .btn-secondary:hover {\n            border-color: var(--color-coral);\n            color: var(--color-coral);\n            background: var(--color-warm-cream);\n        }\n\n        .hero-image-wrapper {\n            position: relative;\n        }\n\n        .hero-main-card {\n            background: white;\n            border-radius: var(--border-radius-large);\n            overflow: hidden;\n            box-shadow: var(--shadow-strong);\n            position: relative;\n        }\n\n        .hero-main-card img {\n            width: 100%;\n            height: 520px;\n            object-fit: cover;\n        }\n\n        .hero-overlay {\n            position: absolute;\n            bottom: 0;\n            left: 0;\n            right: 0;\n            background: linear-gradient(transparent 0%, rgba(26, 42, 58, 0.85) 100%);\n            padding: var(--spacing-lg);\n            color: white;\n        }\n\n        .hero-overlay-tag {\n            display: inline-flex;\n            align-items: center;\n            gap: var(--spacing-xs);\n            background: rgba(232, 120, 106, 0.9);\n            padding: 0.4rem 0.875rem;\n            border-radius: var(--border-radius-pill);\n            font-size: 0.75rem;\n            font-weight: 700;\n            text-transform: uppercase;\n            letter-spacing: 0.08em;\n            margin-bottom: var(--spacing-xs);\n        }\n\n        .hero-overlay-tag svg {\n            width: 14px;\n            height: 14px;\n            fill: white;\n        }\n\n        .hero-overlay-text {\n            font-weight: 600;\n            font-size: 1.0625rem;\n        }\n\n        .floating-card {\n            position: absolute;\n            background: white;\n            border-radius: var(--border-radius-medium);\n            padding: var(--spacing-md);\n            box-shadow: var(--shadow-medium);\n            animation: float-card 6s ease-in-out infinite;\n        }\n\n        .floating-card-1 {\n            top: -20px;\n            right: -30px;\n            animation-delay: 0s;\n        }\n\n        .floating-card-2 {\n            bottom: 40px;\n            left: -25px;\n            animation-delay: -3s;\n        }\n\n        @keyframes float-card {\n            0%, 100% { transform: translateY(0); }\n            50% { transform: translateY(-15px); }\n        }\n\n        .floating-card-icon {\n            width: 50px;\n            height: 50px;\n            background: var(--gradient-warm);\n            border-radius: 50%;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-bottom: var(--spacing-xs);\n        }\n\n        .floating-card-icon svg {\n            width: 24px;\n            height: 24px;\n            fill: white;\n        }\n\n        .floating-card-text {\n            font-weight: 700;\n            color: var(--color-deep-navy);\n            font-size: 0.9375rem;\n        }\n\n        .floating-card-sub {\n            font-size: 0.8125rem;\n            color: var(--color-soft-gray);\n        }\n\n        /* Problem/Solution Section */\n        .problem-solution {\n            background: white;\n            position: relative;\n        }\n\n        .problem-solution-grid {\n            display: grid;\n            grid-template-columns: repeat(2, 1fr);\n            gap: var(--spacing-xl);\n        }\n\n        .ps-card {\n            border-radius: var(--border-radius-large);\n            padding: var(--spacing-xl);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .ps-card::before {\n            content: '';\n            position: absolute;\n            top: 0;\n            left: 0;\n            width: 6px;\n            height: 100%;\n        }\n\n        .problem-card {\n            background: linear-gradient(135deg, #fff5f3 0%, #ffe8e3 100%);\n        }\n\n        .problem-card::before {\n            background: var(--color-coral);\n        }\n\n        .solution-card {\n            background: linear-gradient(135deg, #f0f7f0 0%, #e5f0e5 100%);\n        }\n\n        .solution-card::before {\n            background: var(--color-sage);\n        }\n\n        .ps-header {\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-md);\n            margin-bottom: var(--spacing-lg);\n        }\n\n        .ps-icon {\n            width: 70px;\n            height: 70px;\n            border-radius: var(--border-radius-medium);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            flex-shrink: 0;\n        }\n\n        .problem-card .ps-icon {\n            background: rgba(232, 120, 106, 0.15);\n        }\n\n        .solution-card .ps-icon {\n            background: rgba(125, 168, 123, 0.15);\n        }\n\n        .ps-icon svg {\n            width: 32px;\n            height: 32px;\n        }\n\n        .problem-card .ps-icon svg {\n            fill: var(--color-coral);\n        }\n\n        .solution-card .ps-icon svg {\n            fill: var(--color-sage);\n        }\n\n        .ps-title {\n            font-family: var(--font-display);\n            font-size: 1.75rem;\n            font-weight: 700;\n            color: var(--color-deep-navy);\n        }\n\n        .ps-list {\n            margin-top: var(--spacing-md);\n        }\n\n        .ps-item {\n            display: flex;\n            align-items: flex-start;\n            gap: var(--spacing-sm);\n            margin-bottom: var(--spacing-md);\n            font-size: 1rem;\n            color: var(--color-soft-gray);\n            line-height: 1.7;\n        }\n\n        .ps-item svg {\n            width: 22px;\n            height: 22px;\n            flex-shrink: 0;\n            margin-top: 2px;\n        }\n\n        .problem-card .ps-item svg {\n            fill: var(--color-coral);\n        }\n\n        .solution-card .ps-item svg {\n            fill: var(--color-sage);\n        }\n\n        /* Services Section */\n        .services {\n            background: var(--color-warm-cream);\n            position: relative;\n        }\n\n        .section-header {\n            text-align: center;\n            margin-b