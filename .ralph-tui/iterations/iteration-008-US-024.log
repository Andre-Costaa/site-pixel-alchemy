         --color-coral-dark: #d45548;\n            --color-sage: #7da87b;\n            --color-sage-light: #9bc899;\n            --color-sage-dark: #5c8a5a;\n            --color-sky: #7ec8e3;\n            --color-sky-light: #a8dce8;\n            --color-sky-dark: #5aa8c7;\n            --color-warm-cream: #fdf8f3;\n            --color-soft-white: #faf9f6;\n            --color-deep-navy: #1a2a3a;\n            --color-soft-gray: #6b7280;\n            --color-light-gray: #a0aec0;\n\n            --gradient-coral: linear-gradient(135deg, #e8786a 0%, #d45548 100%);\n            --gradient-sage: linear-gradient(135deg, #7da87b 0%, #5c8a5a 100%);\n            --gradient-hero: linear-gradient(180deg, #fdf8f3 0%, #f5efe6 100%);\n            --gradient-warm: linear-gradient(135deg, #f5a99a 0%, #9bc899 100%);\n            --gradient-card: linear-gradient(180deg, #ffffff 0%, #faf8f5 100%);\n\n            --shadow-soft: 0 8px 32px rgba(26, 42, 58, 0.08);\n            --shadow-medium: 0 12px 48px rgba(26, 42, 58, 0.12);\n            --shadow-strong: 0 20px 64px rgba(26, 42, 58, 0.18);\n            --shadow-coral: 0 12px 40px rgba(232, 120, 106, 0.25);\n\n            --border-radius-soft: 12px;\n            --border-radius-medium: 20px;\n            --border-radius-large: 28px;\n            --border-radius-pill: 999px;\n            --border-radius-blob: 60% 40% 30% 70% / 60% 30% 70% 40%;\n\n            --spacing-xs: 0.5rem;\n            --spacing-sm: 1rem;\n            --spacing-md: 1.5rem;\n            --spacing-lg: 2.5rem;\n            --spacing-xl: 4rem;\n            --spacing-2xl: 6rem;\n\n            --font-display: 'Playfair Display', Georgia, serif;\n            --font-body: 'Nunito', system-ui, sans-serif;\n\n            --transition-smooth: 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);\n        }\n\n        * {\n            margin: 0;\n            padding: 0;\n            box-sizing: border-box;\n        }\n\n        html {\n            scroll-behavior: smooth;\n        }\n\n 