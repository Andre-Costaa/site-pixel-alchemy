  });\n  1743→\n  1744→        // Smooth scroll for anchor links\n  1745→        document.querySelectorAll('a[href^=\"#\"]').forEach(anchor => {\n  1746→            anchor.addEventListener('click', function (e) {\n  1747→                e.preventDefault();\n  1748→                const target = document.querySelector(this.getAttribute('href'));\n  1749→                if (target) {\n  1750→                    const navHeight = nav.offsetHeight;\n  1751→                    const targetPosition = target.offsetTop - navHeight;\n  1752→                    window.scrollTo({\n  1753→                        top: targetPosition,\n  1754→                        behavior: 'smooth'\n  1755→                    });\n  1756→                }\n  1757→            });\n  1758→        });\n  1759→\n  1760→        // Intersection Observer for animations\n  1761→        const observerOptions = {\n  1762→            threshold: 0.1,\n  1763→            rootMargin: '0px 0px -50px 0px'\n  1764→        };\n  1765→\n  1766→        const observer = new IntersectionObserver((entries) => {\n  1767→            entries.forEach(entry => {\n  1768→                if (entry.isIntersecting) {\n  1769→                    const delay = entry.target.dataset.delay || 0;\n  1770→                    setTimeout(() => {\n  1771→                        entry.target.classList.add('animated');\n  1772→                    }, delay);\n  1773→                    observer.unobserve(entry.target);\n  1774→                }\n  1775→            });\n  1776→        }, observerOptions);\n  1777→\n  1778→        document.querySelectorAll('.wow-fade-up, .wow-fade-in, .wow-scale-up').forEach(el => {\n  1779→            observer.observe(el);\n  1780→        });\n  1781→\n  1782→        // Form submission\n  1783→        const contactForm = document.getElementById('contact-form');\n  1784→        const formSuccess = document.getElementById('form-success');\n  1785→\n  1786→        contactForm.addEventListener('submit', (e) =