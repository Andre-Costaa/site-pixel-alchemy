     align-items: center;\n            justify-content: space-between;\n            padding: var(--spacing-sm) var(--spacing-md);\n            max-width: 1200px;\n            margin: 0 auto;\n        }\n\n        .nav-logo {\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-sm);\n            font-family: var(--font-display);\n            font-size: 1.125rem;\n            font-weight: 700;\n            color: var(--color-primary-dark);\n        }\n\n        .nav-logo svg {\n            width: 36px;\n            height: 36px;\n            color: var(--color-primary);\n        }\n\n        .nav-menu {\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-lg);\n        }\n\n        .nav-link {\n            font-weight: 500;\n            color: var(--color-charcoal);\n            position: relative;\n            font-size: 0.9375rem;\n        }\n\n        .nav-link::after {\n            content: '';\n            position: absolute;\n            bottom: -4px;\n            left: 0;\n            width: 0;\n            height: 2px;\n            background: var(--color-primary);\n            transition: var(--transition-base);\n        }\n\n        .nav-link:hover::after {\n            width: 100%;\n        }\n\n        .nav-toggle {\n            display: none;\n            flex-direction: column;\n            gap: 5px;\n            padding: var(--spacing-xs);\n        }\n\n        .nav-toggle span {\n            width: 24px;\n            height: 2px;\n            background: var(--color-charcoal);\n            border-radius: 2px;\n            transition: var(--transition-base);\n        }\n\n        .nav-toggle.active span:nth-child(1) {\n            transform: rotate(45deg) translate(5px, 5px);\n        }\n\n        .nav-toggle.active span:nth-child(2) {\n            opacity: 0;\n        }\n\n        .nav-toggle.active span:nth-child(3) {\n            transform: rotate(-45deg) translate(5px, -5px);\n        }\n\n        @media (max-width: 768px) {\n            .nav-toggle {\n                display: flex;\n            }\n\n            .nav-menu {\n                position: absolute;\n                top: 100%;\n                left: 0;\n                right: 0;\n                background: var(--color-white);\n                flex-direction: column;\n                padding: var(--spacing-lg);\n                gap: var(--spacing-md);\n                transform: translateY(-100%);\n                opacity: 0;\n                visibility: hidden;\n                transition: var(--transition-base);\n            }\n\n            .nav-menu.active {\n                transform: translateY(0);\n                opacity: 1;\n                visibility: visible;\n            }\n        }\n\n        /* Hero Section */\n        .hero {\n            min-height: 100vh;\n            display: flex;\n            align-items: center;\n            padding-top: 80px;\n            background: var(--gradient-hero);\n            position: relative;\n            overflow: hidden;\n        }\n\n        .hero-paw-prints {\n            position: absolute;\n            inset: 0;\n            overflow: hidden;\n            pointer-events: none;\n        }\n\n        .hero-paw {\n            position: absolute;\n            opacity: 0.08;\n            animation: paw-fade 15s ease-in-out infinite;\n        }\n\n        .hero-paw-1 { top: 10%; left: 5%; animation-delay: 0s; }\n        .hero-paw-2 { top: 30%; right: 10%; animation-delay: -3s; }\n        .hero-paw-3 { top: 60%; left: 15%; animation-delay: -6s; }\n        .hero-paw-4 { bottom: 20%; right: 20%; animation-delay: -9s; }\n        .hero-paw-5 { top: 45%; left: 40%; animation-delay: -12s; }\n\n        @keyframes paw-fade {\n            0%, 100% { opacity: 0.05; transform: scale(1); }\n            50% { opacity: 0.15; transform: scale(1.1); }\n        }\n\n        .hero-container {\n            display: grid;\n            grid-template-columns: 1fr 1fr;\n            gap: var(--spacing-2xl);\n            align-items: center;\n            position: relative;\n            z-index: 1;\n        }\n\n        .hero-content {\n            animation: fade-up 0.8s ease-out;\n        }\n\n        .hero-badges {\n            display: flex;\n            gap: var(--spacing-sm);\n            margin-bottom: var(--spacing-md);\n            flex-wrap: wrap;\n        }\n\n        .hero-badge {\n            display: inline-flex;\n            align-items: center;\n            gap: var(--spacing-xs);\n            background: rgba(230, 126, 34, 0.15);\n            color: var(--color-primary-dark);\n            padding: var(--spacing-xs) var(--spacing-md);\n            border-radius: var(--border-radius-pill);\n            font-size: 0.875rem;\n            font-weight: 500;\n        }\n\n        .hero-badge svg {\n            width: 16px;\n            height: 16px;\n        }\n\n        .hero-title {\n            font-family: var(--font-display);\n            font-size: clamp(2.5rem, 6vw, 3.75rem);\n            font-weight: 800;\n            color: var(--color-charcoal);\n            line-height: 1.1;\n            margin-bottom: var(--spacing-md);\n        }\n\n        .hero-title span {\n            background: var(--gradient-warm);\n            -webkit-background-clip: text;\n            -webkit-text-fill-color: transparent;\n            background-clip: text;\n        }\n\n        .hero-description {\n            font-size: clamp(1rem, 2vw, 1.125rem);\n            color: var(--color-gray-light);\n            margin-bottom: var(--spacing-lg);\n            line-height: 1.7;\n        }\n\n        .hero-ctas {\n            display: flex;\n            gap: var(--spacing-sm);\n            flex-wrap: wrap;\n        }\n\n        .hero-image {\n            position: relative;\n            animation: fade-in 1s ease-out 0.3s both;\n        }\n\n        .hero-image-main {\n            position: relative;\n            border-radius: var(--border-radius-lg);\n            overflow: hidden;\n            box-shadow: var(--shadow-lg);\n        }\n\n        .hero-image-main img {\n            width: 100%;\n            height: 500px;\n            object-fit: cover;\n        }\n\n        .hero-float-card {\n            position: absolute;\n            background: var(--color-white);\n            border-radius: var(--border-radius-md);\n            padding: var(--spacing-md);\n            box-shadow: var(--shadow-lg);\n            display: flex;\n            align-items: center;\n            gap: var(--spacing-sm);\n            animation: float 6s ease-in-out infinite;\n        }\n\n        .hero-float-card-1 {\n            bottom: 30px;\n            left: -30px;\n        }\n\n        .hero-float-card-2 {\n            top: 30px;\n            right: -30px;\n            animation-delay: -3s;\n        }\n\n        .float-icon {\n            width: 48px;\n            height: 48px;\n            background: var(--gradient-warm);\n            border-radius: var(--border-radius-sm);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            flex-shrink: 0;\n        }\n\n        .float-icon svg {\n            width: 24px;\n            height: 24px;\n            color: var(--color-white);\n        }\n\n        .float-text strong {\n            display: block;\n            font-family: var(--font-display);\n            font-size: 1.5rem;\n            font-weight: 700;\n            color: var(--color-charcoal);\n        }\n\n        .float-text span {\n            font-size: 0.875rem;\n            color: var(--color-gray-light);\n        }\n\n        @keyframes float {\n            0%, 100% { transform: translateY(0); }\n            50% { transform: translateY(-15px); }\n        }\n\n        @keyframes fade-up {\n            from {\n                opacity: 0;\n                transform: translateY(30px);\n            }\n            to {\n        